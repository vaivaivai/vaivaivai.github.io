/**
 *
 * http://www.anotherhome.net
 * Copyright 2015, DIYgod
 * Free to use under the MIT license.
 *
 */
(function ($) {

  $.carrousel = function (options, element) {
    this.$el = $(element);
    this._init(options);
  };

  $.carrousel.defaults = {
    current: 0,	// index of current item
    autoplay: true,// slideshow on / off
    interval: 3000  // time between transitions
  };

  $.carrousel.prototype = {
    _init: function (options) {
      this.options = $.extend(true, {}, $.carrousel.defaults, options);

      this.$items = $('.content-block__slider-item');
      this.itemsCount = this.$items.length;
      this.$navNext = this.$el.find('.feature__button_next-slide');
      this.button = $('.content-block__slide-bullets');
      this.box = $('.content-block__slider-wrapper');
      this.imgWidth = $('.content-block__slider-item .content-block__background-img').width();

      this.indexB = 0;
      // convert 10% to px for translate
      this.CSSX = this.imgWidth * 0.1;

      this.button.find('li[index=0]').addClass('content-block__bullet-item_active');
      this.$items.addClass('carrousel-transition');

      this.current = this.options.current;
      this.isAnim = false;

      this._layout();
      // load the events
      this._loadEvents();
      // slideshow
      if (this.options.autoplay) {
        this._startSlideshow();
      }
    },

    // выделяет булит в зависимости от выбранного слайда
    _showButton: function () {
      var _self = this,
          lights = _self.button.find('.content-block__bullet-item_active');
      if(lights){
          lights.removeClass('content-block__bullet-item_active');
      }

      _self.button.find('li[index=' + _self.indexB + ']').addClass('content-block__bullet-item_active');
    },

    // устанавливает элементам позицию
    _layout: function () {
      this._setItems();

      this.$leftItm.css(this._getCoordinates('left'));
      this.$rightItm.css(this._getCoordinates('right'));

      this.$currentItm.css(this._getCoordinates('center')).addClass('dg-center');
    },

    // определяет нумерацию элементов
    _setItems: function () {

      this.$items.removeClass('dg-center');

      this.$currentItm    = this.$items.eq( this.current );
      this.$leftItm       = ( this.current === 0 ) ? this.$items.eq( this.itemsCount - 1 ) : this.$items.eq( this.current - 1 );
      this.$rightItm      = ( this.current === this.itemsCount - 1 ) ? this.$items.eq( 0 ) : this.$items.eq( this.current + 1 );

      // next & previous items
      if( this.itemsCount > 3 ) {
        // next item
        this.$nextItm       = ( this.$rightItm.index() === this.itemsCount - 1 ) ? this.$items.eq( 0 ) : this.$rightItm.next();
        this.$nextItm.css( this._getCoordinates('outright') );

        // previous item
        this.$prevItm       = ( this.$leftItm.index() === 0 ) ? this.$items.eq( this.itemsCount - 1 ) : this.$leftItm.prev();
        this.$prevItm.css( this._getCoordinates('outleft') );
      }
    },

    _loadEvents: function () {
      var _self = this;
      // событие клика по булитам
     _self.button.children().on('click', function() {
        var toIndex = parseInt(this.getAttribute('index'));
        var toMove = toIndex - _self.indexB;
        switch (toMove) {
          case 0:
            break;
          case 1:
            _self._navigate('next');
            break;
          case -1:
            _self._navigate('prev');
            break;
          default:
            _self._stopSlideshow();
            var bTime = setInterval(function () {
              if (!_self.isAnim) {
                if (!toMove) {
                  clearInterval(bTime);
                  _self.$items.removeClass('carrousel-transition-fast');
                  if (_self.options.autoplay) {
                    _self._startSlideshow();
                  }
                }
                else{
                  _self.$items.addClass('carrousel-transition-fast');
                    if (toMove > 0) {
                        _self._navigate('next');
                        toMove--;
                    }
                    else if (toMove < 0) {
                        _self._navigate('prev');
                        toMove++;
                    }
                }
              }
            }, 200);
            break;
        }
      });

      this.$navNext.on('click.carrousel', function () {
        _self._navigate('next');
        return false;
      });

      $(window).on('resize', function(){
        var imgWidth = $('.content-block__slider-item .content-block__background-img').width();
        // convert 10% to px for translate and apply
        _self.CSSX = imgWidth * 0.1;
        _self._layout();
      });

      this.$el.on('webkitTransitionEnd.carrousel transitionend.carrousel OTransitionEnd.carrousel', function () {
        _self.$currentItm.addClass('dg-center');
        _self.isAnim    = false;
      });
    },

    // получение координат слайдов
    _getCoordinates: function (position) {
        switch (position) {
          case 'outleft':
            return {
              '-webkit-transform': 'translate(0px) scale(0.8)',
              '-moz-transform': 'translate(0px) scale(0.8)',
              '-o-transform': 'translate(0px) scale(0.8)',
              '-ms-transform': 'translate(0px) scale(0.8)',
              'transform': 'translate(0px) scale(0.8)',
              'opacity': 1,
              'z-index': 2
            };
            break;
          case 'outright':
            return {
              '-webkit-transform': 'translate(0px) scale(0.8)',
              '-moz-transform': 'translate(0px) scale(0.8)',
              '-o-transform': 'translate(0px) scale(0.8)',
              '-ms-transform': 'translate(0px) scale(0.8)',
              'transform': 'translate(0px) scale(0.8)',
              'opacity': 1,
              'z-index': 2
            };
            break;
          case 'left':
            return {
              '-webkit-transform': 'translate(-' + this.CSSX + 'px) scale(0.9)',
              '-moz-transform': 'translate(-' + this.CSSX + 'px) scale(0.9)',
              '-o-transform': 'translate(-' + this.CSSX + 'px) scale(0.9)',
              '-ms-transform': 'translate(-' + this.CSSX + 'px) scale(0.9)',
              'transform': 'translate(-' + this.CSSX + 'px) scale(0.9)',
              'opacity': 1,
              'visibility': 'visible',
              'z-index': 3
            };
            break;
          case 'right':
            return {
              '-webkit-transform': 'translate(' + this.CSSX + 'px) scale(0.9)',
              '-moz-transform': 'translate(' + this.CSSX + 'px) scale(0.9)',
              '-o-transform': 'translate(' + this.CSSX + 'px) scale(0.9)',
              '-ms-transform': 'translate(' + this.CSSX + 'px) scale(0.9)',
              'transform': 'translate(' + this.CSSX + 'px) scale(0.9)',
              'opacity': 1,
              'visibility': 'visible',
              'z-index': 3
            };
            break;
          case 'center':
            return {
              '-webkit-transform': 'translate(0px) scale(1)',
              '-moz-transform': 'translate(0px) scale(1)',
              '-o-transform': 'translate(0px) scale(1)',
              '-ms-transform': 'translate(0px) scale(1)',
              'transform': 'translate(0px) scale(1)',
              'opacity': 1,
              'visibility': 'visible',
              'z-index': 10
            };
          case 'hide':
            return {
              '-webkit-transform': 'translate(0px) scale(0.7)',
              '-moz-transform': 'translate(0px) scale(0.7)',
              '-o-transform': 'translate(0px) scale(0.7)',
              '-ms-transform': 'translate(0px) scale(0.7)',
              'transform': 'translate(0px) scale(0.7)',
              'opacity': 1,
              'visibility': 'visible',
              'z-index': 1
            };
            break;
        }

    },

    _navigate: function (dir) {
      if (!this.isAnim) {
        this.isAnim = true;

        switch (dir) {
          case 'next' :
            this.indexB++;
            if (this.indexB === this.itemsCount) {
              this.indexB = 0;
            }
            this._showButton();
            this.current = this.$rightItm.index();
            // current item moves left
            this.$currentItm.css(this._getCoordinates('left'));

            // right item moves to the center
            this.$rightItm.css(this._getCoordinates('center'));

            // next item moves to the right
            if( this.$nextItm ) {

              // left item moves out
              this.$leftItm.css( this._getCoordinates('outleft') );

              this.$nextItm.css( this._getCoordinates('right') );

            }
            else {

              // left item moves right
              this.$leftItm.css( this._getCoordinates('right') );

            }
            break;

          case 'prev' :
            this.indexB--;
            if (this.indexB === -1) {
              this.indexB = this.itemsCount - 1;
            }
            this._showButton();
            this.current = this.$leftItm.index();
            // current item moves right
            this.$currentItm.css(this._getCoordinates('right'));

            // left item moves to the center
            this.$leftItm.css(this._getCoordinates('center'));

            // prev item moves to the left
            if( this.$prevItm ) {

              // right item moves out
              this.$rightItm.css( this._getCoordinates('outright') );

              this.$prevItm.css( this._getCoordinates('left') );

            }
            else {

              // right item moves left
              this.$rightItm.css( this._getCoordinates('left') );

            }
            break;
        }
        this._setItems();
      }
    },

    // 自动切换
    _startSlideshow: function () {
      if (this.slideshow) {
        clearInterval(this.slideshow);
      }
      var _self = this;
      this.slideshow = setInterval(function () {
        if ($('.dg-center')[0] && !_self.isAnim) {
          _self._navigate('next');
        }
      }, this.options.interval);
    },

    _stopSlideshow: function () {
      clearTimeout(this.slideshow);
    }
  };

  $.fn.carrousel = function (options) {
    if (typeof options === 'object') {
      this.each(function () {
        var instance = $.data(this, 'carrousel');
        if (!instance) {
          $.data(this, 'carrousel', new $.carrousel(options, this));
        }
      });
    }
    else if (typeof options === 'string') {
      this.each(function () {
        var instance = $.data(this, 'carrousel');
        if (instance) {
          switch (options) {
            case 'play':
              instance._startSlideshow();
              instance.options.autoplay = true;
              break;
            case 'stop':
              instance._stopSlideshow();
              instance.options.autoplay = false;
              break;
            case 'next':
              instance._navigate('next');
              break;
            case 'prev':
              instance._navigate('prev');
              break;
          }
        }
      });
    }
    else if (typeof options === 'number') {
      this.each(function () {
        var instance = $.data(this, 'carrousel');
        instance.button[options].click();
      });
    }
    return this;
  };

})(jQuery);

/**
 * jQuery Plugin to add basic "swipe" support on touch-enabled devices
 *
 * @author Yair Even Or
 * @version 1.0.0 (March 20, 2013)
 */
(function($) {
    $.event.special.swipe = {
        setup: function(){
            $(this).on('touchstart', $.event.special.swipe.handler);
        },

        teardown: function(){
            $(this).off('touchstart', $.event.special.swipe.handler);
        },

        handler: function(event){
            var args = [].slice.call( arguments, 1 ), // clone arguments array, remove original event from cloned array
                touches = event.originalEvent.touches,
                startX, startY,
                deltaX = 0, deltaY = 0,
                that = this;

            event = $.event.fix(event);

            if(touches.length == 1){
                startX = touches[0].pageX;
                startY = touches[0].pageY;
                this.addEventListener('touchmove', onTouchMove, false);
                this.addEventListener('touchend', cancelTouch, false);
            }

            function cancelTouch(){
                that.removeEventListener('touchmove', onTouchMove);
                that.removeEventListener('touchend', cancelTouch);
                startX = startY = null;
            }

            function onTouchMove(e){
                var Dx = startX - e.touches[0].pageX,
                    Dy = startY - e.touches[0].pageY,
                    swipe_x = Math.abs(Dx) >= 20,
                    swipe_y = Math.abs(Dy) >= 20;

                if (swipe_x || swipe_y){
                    cancelTouch();
                    deltaX = swipe_x ? ((Dx > 0) ? -1 : 1) : 0;
                    deltaY = swipe_y ? ((Dy > 0) ? 1 : -1) : 0;
                }

                if (deltaX !== 0 && $(that).hasClass('js-swipe-prevent-x')) {
                    e.preventDefault();
                }

                event.type = "swipe";
                args.unshift(event, deltaX, deltaY, e.touches.length); // add back the new event to the front of the arguments with the delatas
                return ($.event.dispatch || $.event.handle).apply(that, args);
            }
        }
    };
})(window.jQuery || window.Zepto);
var AmoValidate = AMOCRM.AmoValidate || {};

$(function(){
  'use strict';

  var country_list = {
        ru: ['ru', 'by', 'ua', 'kz'],
        en: ['us', 'gb', 'au', 'in', 'ca'],
        es: ['us', 'mx', 'es', 'ec', 'cl', 'ar', 'gt']
      },
      email_pattern = /^[a-z0-9!#$%&'*+\/=?^_`{|}~-]+(?:\.[a-z0-9!#$%&'*+\/=?^_`{|}~-]+)*@(?:[\da-z](?:[\da-z-_]*[\da-z])?\.)+[a-z]{2,}$/i;

  /**
   * [AmoValidate конструктор валидации]
   * @param {object} options принимает в качестве
   * параметров селектор формы, селектор инпутов,
   * колбек для кнопки сабмита формы,
   * флаг проверки существования почты в бд
   */
  AmoValidate = function (options) {
    this.initialize(options);
  };

  _.extend(AmoValidate.prototype, {
    initialize: function (options) {
      this.options = options || {};
      this.$form = $(options.form_selector);
      this.$inputs = this.$form.find('.field ' + options.input_selector);
      this.$checkbox = this.$form.find(options.checkbox_selector);
      this.submit_blocked = false;
      this.values = options.values || {};

      // Init this module
      this.init();
    },

    init: function () {
      this.bindDomEvents();
      var $tel_wrapper = this.$form.find('.field[data-type="tel"]'),
          $tel_input = $tel_wrapper.find(this.options.input_selector);

      if ($tel_wrapper.length) {
        this.validationCleanup($tel_input, $tel_wrapper);

        if (!$tel_wrapper.find('.intl-tel-input').length) {
          this.intlTelInputConfig($tel_input);
        }
      }
    },

    fixTab:function(){
      $('.flag-container *').attr('tabindex', -1);
    },

    intlTelInputConfig: function ($tel_input) {
      var self = this,
          defCountry = AMOCRM.geo.country_code,
          prefCountries = country_list[AMOCRM.geo.language],
          countryCode, has_country;

      has_country = $.fn.intlTelInput.getCountryData().some(function (country) {
        return country.iso2 == defCountry;
      });
      defCountry = has_country ? defCountry : 'ru';
      $tel_input.intlTelInput({
        utilsScript: '/design/summer_16/js/bower_components/intl-tel-input/lib/libphonenumber/build/utils.js',
        defaultCountry: defCountry,
        preferredCountries: prefCountries,
        nationalMode: true,
        initialCountry: defCountry,
        autoPlaceholder: true,
        autoFormat: true,
        geoIpLookup: function(callback) {
          if (Cookies.get('amo_geo') === undefined) {
            countryCode = AMOCRM.geo.country;
          } else {
            var countryCode_info = Cookies.get('amo_geo');
            var country = JSON.parse(countryCode_info).country;
            countryCode = country == undefined ? AMOCRM.geo.country : country;
          }
          callback(countryCode);
        }
      });

      setTimeout(self.fixTab, 500);
    },

    bindDomEvents: function () {
      var self = this,
          callback;

      this.$form
        .on('input', '.field ' + this.options.input_selector, _.debounce(function (e) {
          self.checkInput(e);
        }, 500))
        .on('submit', function (e) {
          self.onFormSubmit(e);
        });

      if (this.options.checkbox_selector) {
        this.$form.on('change', this.options.checkbox_selector, function(e) {
          callback = self.options.checkbox_callback;

          if (_.isFunction(self.checkbox_callbacks[callback])) {
            self.checkbox_callbacks[callback].call(self, e);
          }
        });

        this.$checkbox.prop('checked', true);
      }

      if($('#account_coupon_code').val()) {
        $('.show_when_have_coupone').show();
        $('#do_you_have_coupone').hide();
      }

      $('#do_you_have_coupone').click(function(e){
        $(this).css('display', 'none');
        $('.show_when_have_coupone').show();
      });

    },

    // костыль под старые IE
    // инициализация плагина с номерами телефона меняет плейсхолдер,
    // триггерит событие инпут.
    // также убирает tabindex
    validationCleanup: function ($input, $wrapper) {
      this.$form.find('.selected-flag').prop('tabindex', '-1');

      if ($('html').hasClass('no-cssfilters')) {
        $input.one('input', function(e) {
          setTimeout(function(){
            $wrapper.removeClass('invalid_field');
          }, 600);
        });
      }
    },

    onFormSubmit: function (e) {
      var self = this,
          promises = [];

      e.preventDefault();
      e.stopPropagation();

      if (!this.submit_blocked) {
        _.each(this.$inputs, function (input) {
          var type = $(input).prop('type');

          if (type !== self.inputs_skip) {
            promises.push(self.checkInput(e, input));
          }
        });

        $.when.apply($, promises).then(_.bind(function () {
            this.submit_blocked = true;
            this.submit_callbacks[this.options.submit_callback].call(this, e);
        }, this));
      }
    },

    submit_callbacks: {
      form_blog_sbcr: function (e) {
        $('.popup-sub').show();
        $('.bottom-sub-inner').hide();
        setTimeout(function () {
          $('.bottom-sub').slideUp();
        }, 2500);
        setTimeout(function () {
          $('.bottom-sub').remove();
        }, 2000);
        $.ajax({
          url: '/private/blog_subscribe.php',
          method: 'POST',
          data: {
            sub_email: this.values.email
          }
        }).done(function (response) {
          if (response.error_code == 0) {
            Cookies.set('amo_blog_sub', 1, {expires: 900});
          } else {
            Cookies.set('amo_blog_sub', 1, {expires: 1});
          }
        });
      },

      form_start: function (e) {
        var self = this,
            $submit = this.$form.find('[type="submit"]'),
            reg_url = '/account/registration.php',
            //checked = this.$checkbox.prop('checked'),
            data = {
              ACTION: 'REGISTER_NEW_ACCOUNT',
              need_json_response: true,
              recaptcha: $(document).find('#g-recaptcha-response').val(),
              account: {
                user_name: this.values.email,
                first_name: this.values.name,
                phone_num: this.values.tel,
                assistance: 0,
                visitor_uid: AMOCRM.getVisitorUid(),
                timezone: this.$form.find('.timezone').val(),
                country: this.$form.find('.country_reg').val()
              },
              coupon_code: this.$form.find('#account_coupon_code').val()
            },
            error_message = $('.try_now__button_submit_error');

        $submit.addClass('button_progress');
        error_message.css('visibility', 'hidden');

        $.ajax({
          type: 'POST',
          url: (AMOCRM.geo.language === 'es' ? '/es' : '') + reg_url,
          dataType: 'json',
          data: data
        }).always(function () {
          $submit.removeClass('button_progress');
          self.submit_blocked = false;
        }).done(function (response) {
          if (response['response']['error'] !== undefined) {
              error_message.css('visibility', 'visible');
          } else {
              var subdomain = response['response']['accounts'][0]['subdomain'];
              if (location.pathname === '/tour/') {
                  fbq('track', 'CompleteRegistration');
                  yaCounter561575.reachGoal('new_account');
              }
              window.location = 'http://' + subdomain + location.host.replace(/^\w+/, '') + '/dashboard/?WELCOME=Y';
          }
        }).fail(function() {
            error_message.css('visibility', 'visible');
        });

        if (_.isFunction(window.goog_report_conversion)) {
          window.goog_report_conversion ('https://www.amocrm.ru')
        }
      },

      cloud_form: function (e) {
        var self = this,
            $submit = this.$form.find('[type="submit"]'),
            reg_url = '/partners/create_partner.php',
            data = {
              name: this.values.name,
              email: this.values.email,
              telephone: this.values.tel,
              company: this.values.company,
              contract_data: this.values.contract_data,
              partners_code: this.values.partners_code,
              dealer_1c: Number(this.$form.find('#dealer_1c')[0].checked),
              submit: true
            };

        $submit.addClass('button_progress');

        $.ajax({
          type: 'POST',
          url: reg_url,
          dataType: 'json',
          data: data
        }).always(function () {
          $submit.removeClass('button_progress');
          self.submit_blocked = false;
        }).done(function (response) {
          var msg;
          if (response.error) {
            msg = response.error;
          } else {
            if (response.partner_exist === 'y') {
              msg = 'Такой партнер уже существует в системе!';
            } else {
              msg = response.success;
            }
          }

          // этот алерт не трогать! Он нужен для информирования менеджера 1С
          alert(msg);
          if (response.success) {
            location.reload();
          }
        });
      },

      form_auth: function (e) {
        var self = this,
            $submit = this.$form.find('[type="submit"]'),
            data = {
              csrf_token: this.$form.find('[name="csrf_token"]').val(),
              password: this.values.password,
              username: this.values.email
            },
            message;

        $submit.addClass('button_progress');
        self.$form
            .find('.form_auth__notify_error_forgot')
            .addClass('hidden');

        if (this.$form.find('#UserRemember').prop('checked')) {
          data.temporary_auth = 'Y';
        }

        $.ajax({
          url: '/oauth2/authorize',
          method: 'POST',
          headers: {
            Accept: 'application/json'
          },
          contentType: 'application/json',
          data: JSON.stringify(data)
        })
        .always(function() {
          $submit.removeClass('button_progress');
          self.submit_blocked = false;
        })
        .done(function (res) {
          location.reload();
        })
        .fail(function() {
          self.$form
            .find('.form_auth__notify_error_forgot')
            .removeClass('hidden');

          self.$form
            .find('.form_auth__notify')
            .show()
            .find('.form_auth__notify_error-message')
            .html(message);
        });
      },

      form_forgot: function (e) {
        var self = this,
            url = '/account/auth_ajax.php?forgot_password=yes';

        this.$form.find('#forgotpass_sended, #forgotpass_error').addClass('hidden');

        $.ajax({
          url: (AMOCRM.geo.language === 'es' ? '/es' : '') + url,
          method: 'POST',
          data: 'USER_LOGIN='+this.values.email+'&AUTH_FORM=Y&TYPE=SEND_PWD'
        }).always(function (res) {
          self.submit_blocked = false;
        }).done(function (response) {
          // коды жестко вшиты в компоненте, а сами константы определяются в auth_ajax.php
          if (response.match('f4fe5gh78f5d34f45g6h67jhgf4wddf45g56h7')) {
            self.$form.find('#forgotpass_sended').removeClass('hidden');
          } if (response.match('cvkobf7q9mun8676u8yvbni9f')) {
            self.$form.find('#forgotpass_error').removeClass('hidden');
          }
        });
      }
    },

    checkbox_callbacks: {
      assistance: function (e) {
        var $tel = this.$form.find('.field[data-type="tel"]'),
            $tel_input = $tel.find('input[type="tel"]'),
            disabled = !$(e.target).prop('checked');

        if (disabled) {
          $tel.removeClass('valid_field invalid_field');
        } else if ($tel_input.val().length) {
          $tel_input.trigger('input');
        }

        this.inputs_skip = disabled ? 'tel' : null;
        this.tel_input_disabled = disabled;
        $tel.toggleClass('disabled_field', disabled);
        $tel_input.prop('disabled', disabled);
      }
    },

    payer_num_checker: {
      inn_legal: function (value) {
        if (value.length === 10 && value.match(/\d+/)) {
          var n =
            2 * parseInt(value.charAt(0))
            + 4 * parseInt(value.charAt(1))
            + 10 * parseInt(value.charAt(2))
            + 3 * parseInt(value.charAt(3))
            + 5 * parseInt(value.charAt(4))
            + 9 * parseInt(value.charAt(5))
            + 4 * parseInt(value.charAt(6))
            + 6 * parseInt(value.charAt(7))
            + 8 * parseInt(value.charAt(8));
          n = n % 11;
          n = n % 10;

          return parseInt(value.charAt(9)) === n;
        }
        return false;
      },

      inn_individual: function (value) {
        if (value.length === 12 && /\d+/.test(value)) {
          var n1 =
            7 * parseInt(value.charAt(0))
            + 2 * parseInt(value.charAt(1))
            + 4 * parseInt(value.charAt(2))
            + 10 * parseInt(value.charAt(3))
            + 3 * parseInt(value.charAt(4))
            + 5 * parseInt(value.charAt(5))
            + 9 * parseInt(value.charAt(6))
            + 4 * parseInt(value.charAt(7))
            + 6 * parseInt(value.charAt(8))
            + 8 * parseInt(value.charAt(9));
          n1 = n1 % 11;
          n1 = n1 % 10;

          var n2 =
            3 * parseInt(value.charAt(0))
            + 7 * parseInt(value.charAt(1))
            + 2 * parseInt(value.charAt(2))
            + 4 * parseInt(value.charAt(3))
            + 10 * parseInt(value.charAt(4))
            + 3 * parseInt(value.charAt(5))
            + 5 * parseInt(value.charAt(6))
            + 9 * parseInt(value.charAt(7))
            + 4 * parseInt(value.charAt(8))
            + 6 * parseInt(value.charAt(9))
            + 8 * parseInt(value.charAt(10));
          n2 = n2 % 11;
          n2 = n2 % 10;

          return parseInt(value.charAt(10)) === n1 && parseInt(value.charAt(11)) === n2;
        }
        return false;
      },

      unp: function (value) {
        if (value.length === 9 && /\d+/.test(value)) {
          var n =
            29 * parseInt(value.charAt(0))
            + 23 * parseInt(value.charAt(1))
            + 19 * parseInt(value.charAt(2))
            + 17 * parseInt(value.charAt(3))
            + 13 * parseInt(value.charAt(4))
            + 7 * parseInt(value.charAt(5))
            + 5 * parseInt(value.charAt(6))
            + 3 * parseInt(value.charAt(7))
          n = n % 11;

          return parseInt(value.charAt(8)) === n;
        }
        return false;
      },

      bin: function (value) {
        if (value.length === 12 && /\d+/.test(value)) {
          var n =
              1 * parseInt(value.charAt(0))
              + 2 * parseInt(value.charAt(1))
              + 3 * parseInt(value.charAt(2))
              + 4 * parseInt(value.charAt(3))
              + 5 * parseInt(value.charAt(4))
              + 6 * parseInt(value.charAt(5))
              + 7 * parseInt(value.charAt(6))
              + 8 * parseInt(value.charAt(7))
              + 9 * parseInt(value.charAt(8))
              + 10 * parseInt(value.charAt(9))
              + 11 * parseInt(value.charAt(10)),
            k = n % 11;

          if (k === 10) {
            n =
              3 * parseInt(value.charAt(0))
              + 4 * parseInt(value.charAt(1))
              + 5 * parseInt(value.charAt(2))
              + 6 * parseInt(value.charAt(3))
              + 7 * parseInt(value.charAt(4))
              + 8 * parseInt(value.charAt(5))
              + 9 * parseInt(value.charAt(6))
              + 10 * parseInt(value.charAt(7))
              + 11 * parseInt(value.charAt(8))
              + 1 * parseInt(value.charAt(9))
              + 2 * parseInt(value.charAt(10));
            k = n % 11;
          }

          return parseInt(value.charAt(11)) === k && n !== 0;
        }
        return false;
      },

      egrpou: function (value) {
        if (value.length === 8 && /\d+/.test(value)) {
          if (parseInt(val) < 30000000 || parseInt(val) > 60000000) {
            var n =
              1 * parseInt(value.charAt(0))
              + 2 * parseInt(value.charAt(1))
              + 3 * parseInt(value.charAt(2))
              + 4 * parseInt(value.charAt(3))
              + 5 * parseInt(value.charAt(4))
              + 6 * parseInt(value.charAt(5))
              + 7 * parseInt(value.charAt(6));
            k = n % 11;

            if (k === 10) {
              n =
                3 * parseInt(value.charAt(0))
                + 4 * parseInt(value.charAt(1))
                + 5 * parseInt(value.charAt(2))
                + 6 * parseInt(value.charAt(3))
                + 7 * parseInt(value.charAt(4))
                + 8 * parseInt(value.charAt(5))
                + 9 * parseInt(value.charAt(6));
              k = n % 11;
            }

            return parseInt(value.charAt(7)) === k;
          } else {
            var n =
              7 * parseInt(value.charAt(0))
              + 1 * parseInt(value.charAt(1))
              + 2 * parseInt(value.charAt(2))
              + 3 * parseInt(value.charAt(3))
              + 4 * parseInt(value.charAt(4))
              + 5 * parseInt(value.charAt(5))
              + 6 * parseInt(value.charAt(6));
            k = n % 11;

            if (k === 10) {
              n =
                9 * parseInt(value.charAt(0))
                + 3 * parseInt(value.charAt(1))
                + 4 * parseInt(value.charAt(2))
                + 5 * parseInt(value.charAt(3))
                + 6 * parseInt(value.charAt(4))
                + 7 * parseInt(value.charAt(5))
                + 8 * parseInt(value.charAt(6));
              k = n % 11;
            }

            return parseInt(value.charAt(7)) === k;
          }
        }
        return false;
      }
    },

    comparers: {
      name: function (value) {
        var def = $.Deferred(),
            form_type = this.$form.data('type');

        if (value.length < 4 ||
            !value.length ||
            (form_type === 'cloud' && !/[а-яё]/i.test(value))) {
            def.reject();
        } else {
          def.resolve();
        }

        this.values.name = value;

        return def.promise();
      },

      password: function (value) {
        var def = $.Deferred();

        if (!value.length) {
          def.reject();
        } else {
          def.resolve();
        }

        this.values.password = value;

        return def.promise();
      },

      email: function (value) {
        var def = $.Deferred(),
            email_pattern = /^[a-z0-9!#$%&'*+\/=?^_`{|}~-]+(?:\.[a-z0-9!#$%&'*+\/=?^_`{|}~-]+)*@(?:[\da-z](?:[\da-z-_]*[\da-z])?\.)+[a-z]{2,}$/i;

        if (value.length === 0) {
          // проверка на пустой email
          // $email_error.html(AMOCRM.langs.EMPTY_EMAIL);
          def.reject();
        } else if (!email_pattern.test(value)) {
          // проверка на некорректный email
          // $email_error.html(AMOCRM.langs.INCORRECT_EMAIL);
          def.reject();
        } else if (this.options.check_email_existence) {
          // проверка на существование email
          this.checkEmailExistence(value)
            .done(_.bind(function (res) {
              var result = $.parseJSON(res);

              if (result.mail === value) {
                if (result.status === 'used') {

                  if (this.options.switch_mode) {
                    def.resolve();

                    if (this.options.form_type !== 'auth') {
                      this.switchAuth(value);
                    }
                  } else {
                    def.reject();
                    // $email_error.text(AMOCRM.langs.login_used);
                  }
                } else {
                  def.resolve();

                  if (this.options.switch_mode && this.options.form_type !== 'reg') {
                    this.switchReg(value);
                  }
                }
              }

              return def.promise();
            }, this));
        } else {
          def.resolve();
        }

        this.values.email = value;
        if (def.state() === 'rejected' && this.options.switch_mode && this.options.form_type !== 'reg') {
          this.switchReg(value);
        }

        return def.promise();
      },

      partner_email: function (value) {
        var def = $.Deferred();

        if (value.length === 0) { // проверка на пустой email
          def.reject();
        } else if (!email_pattern.test(value)) { // проверка на некорректный email
          def.reject();
        } else if (this.options.check_email_existence) { // проверка на существование партнёра с таким email
          this.checkPartnerExistenceByEmail(value)
            .done(_.bind(function (res) {
              var result = $.parseJSON(res),
                  message = '';

              if (result.error === true) {
                message = 'Произошла ошибка: ' + result.message;
                def.reject();
              } else if (result.partner_exist === true) {
                message = 'Такой партнёр уже сущетсвует';
                def.reject();
              } else if (result.partner_exist === false) {
                $('.form_start__email .form_start__notify_error-message').text('');
                def.resolve();
              }

              if (message !== '') {
                $('.form_start__email .form_start__notify_error-message').text(message);
              }
              return def.promise();
            }, this));
        } else {
          def.resolve();
        }

        this.values.email = value;

        return def.promise();
      },

      tel: function (value, $input) {
        var def = $.Deferred(),
            intlTelInputUtils = window.intlTelInputUtils || {};

        if (!$input.intlTelInput("isValidNumber")) {
          var error = $input.intlTelInput("getValidationError");

          if (error == intlTelInputUtils.validationError.TOO_SHORT) {
            // $phone_error.html(AMOCRM.langs.SMALL_PHONE);
            def.reject();
          } else if (error == intlTelInputUtils.validationError.INVALID_COUNTRY_CODE || error == intlTelInputUtils.validationError.TOO_LONG || error == intlTelInputUtils.validationError.NOT_A_NUMBER) {
            // $phone_error.html(AMOCRM.langs.INCORRECT_PHONE);
            def.reject();
          } else if (value.length === 0) {
            // $phone_error.html(AMOCRM.langs.EMPTY_PHONE);
            def.reject();
          } else {
            // $phone_error.html(AMOCRM.langs.INCORRECT_PHONE);
            def.reject();
          }
        } else {
          this.values.tel = $input.intlTelInput("getNumber");
          def.resolve()
        }

        return def.promise();
      },

      company: function (value) {
        var def = new $.Deferred();

        if (!value.length) {
          def.reject();
        } else {
          def.resolve();
        }

        this.values.company = value;

        return def.promise();
      },

      payer_num: function (value) {
        var def = new $.Deferred(),
            val = $.trim(value),
            options = ['inn_legal', 'inn_individual', 'unp', 'bin', 'egrpou'],
            payer_num_type;

        if (val.length) {
          // сначала проверим, что все валидно и определим тип номера
          _.each(options, function (type) {
            if (this.payer_num_checker[type].call(this, val) && !payer_num_type) {
              payer_num_type = type;
            }
          }, this);

          if (payer_num_type) {
            // если это инн, то нужно дополнительно проверить по реестру
            if (payer_num_type.startsWith('inn')) {
              payer_num_type = 'inn';
              this.values.contract_data = { payer_num_type: payer_num_type };
              $.ajax({
                  type: "POST",
                  headers: {
                      "Authorization": "Token 69977fdb28699ceac8fd4ee798731b20c2f538a1",
                      "Accept": "application/json, text/javascript",
                      "Content-Type": "application/json"
                  },
                  url: "https://dadata.ru/api/v2/suggest/party",
                  dataType: 'json',
                  data: JSON.stringify({query: val}),
                  context: this
              }).done(function (msg) {
                  if (msg.suggestions) {
                      // возьмем первый попавшийся
                      var legal_obj = msg.suggestions[0];
                      if(legal_obj.data) {
                          var contract_obj = {
                            payer_num: legal_obj.data.inn,
                            address: legal_obj.data.address.value
                          };

                          if(legal_obj.data.type === 'LEGAL') {
                            _.extend(contract_obj, {
                              kpp: legal_obj.data.kpp,
                              management: legal_obj.data.management.name,
                              position: legal_obj.data.management.post
                            })
                          }
                          _.extend(this.values.contract_data, contract_obj);
                          def.resolve();
                      }
                  }
                  if (def.state() !== 'resolved') {
                    def.reject();
                  }
              }).fail(function () {
                def.reject();
              });
            } else {
              _.extend(this.values.contract_data, { payer_num: val });
              def.resolve();
            }
          } else {
            def.reject();
          }
        }

        return def.promise();
      },

      partners_code: function (value) {
        var def = new $.Deferred(),
            intRegex = /\d+/;

        if (!value.length || !intRegex.test(value)) {
          def.reject();
        } else {
          def.resolve();
        }

        this.values.partners_code = value;

        return def.promise();
      },

      coupon_code: function (value) {
        var def = new $.Deferred();

        if (value === '') {
          def.resolve();
        }

        $.ajax({
          url: "/account/check_coupon.php",
          method: 'post',
          data: {code: value},
          success: function (data) {
            if (data === 'n') {
              def.reject();
            } else if (data === 'y') {
              def.resolve();
            } else {
              def.resolve();
            }
          }
        });

        return def.promise();
      }
    },

    checkInput: function (e, elem) {
      var $this = elem ? $(elem) : $(e.target),
          $wrapper = $this.closest('.field'),
          type = $wrapper.data('type'),
          value = $this.val().trim(),
          promise = this.comparers[type].call(this, value, $this);

      promise.then(
        function(){
          if (!$wrapper.hasClass('valid_field')) {
            $wrapper.removeClass('invalid_field').addClass('valid_field');
          }
        },
        function(){
          if (!$wrapper.hasClass('invalid_field')) {
              $wrapper.removeClass('valid_field').addClass('invalid_field');
          }
        }
      );

      return promise;
    },

    checkEmailExistence: function (value) {
      return $.ajax({
        type: 'POST',
        url: '/account/check_login.php',
        async: false,
        data: {
          LOGIN: value
        }
      });
    },

    checkPartnerExistenceByEmail: function (value) {
      return $.ajax({
        type: 'POST',
        url: '/partners/private/ajax/check_login/check_login.php',
        data: {
          login: value
        }
      });
    },

    destroy: function (force_exit) {
      this.$form.off();
      this.$form.find('.form_auth__notify').hide();

      _.each(this.$inputs, function (input) {
        var $this = $(input),
            type = $this.prop('type');

        if (type !== 'email' || force_exit) {
          $this.val('').prop('disabled', false).closest('.field')
            .removeClass('valid_field invalid_field disabled_field');
        }
      });
    },

    switchAuth: function (value) {
      this.destroy();
      this.switchForm(true);

      this.initialize.call(this, {
        form_selector: '#new_try_now__form',
        input_selector: '.new_try_now__auth.new_try_now__input',
        submit_callback: 'form_auth',
        check_email_existence: true,
        switch_mode: true,
        form_type: 'auth',
        values: {
          email: value
        }
      });
    },

    switchReg: function (value) {
      this.destroy();
      this.switchForm(false);

      this.initialize.call(this, {
        form_selector: '#new_try_now__form',
        input_selector: '.new_try_now__reg.new_try_now__input',
        submit_callback: 'form_start',
        checkbox_selector: '#new_try_now__free_assist',
        checkbox_callback: 'assistance',
        check_email_existence: true,
        switch_mode: true,
        form_type: 'reg',
        values: {
          email: value
        }
      });
    },

    switchForm: function (show) {
      this.$form.find('.show_when_email_exist').toggle(show);
      this.$form.find('.hide_when_email_exist').toggle(!show);
    }
  });
});

var AmoValidate = AMOCRM.AmoValidate || {};

$(function() {
  'use strict';

  var field = 'yclid';
  var url = window.location.href;
  if (url.indexOf('?' + field + '=') !== -1) {
    $.cookie('yclid', '1', {expires: 3});
  }

  var $document = $(document),
    $window = $(window),
    $body = $('body'),
    $html = $('html'),
    xsMedia = 480,
    smMedia = 768,
    mdMedia = 1024,
    lgMedia = 1280,

    App;

  App = function () {
    this.drag = {};

    this.init();
    sbjs.init({
      lifetime: 12,
      session_length: 30,
      domain: 'amocrm.ru'
    });
  };

  _.extend(App.prototype, {
    init: function() {
      this.collapsing();
      this.navigateToAnchor();
      this.fileUploader();
      this.bindEvents();
      this.initFancyBoxConfig();
      this.initBellowsConfig();
      this.getCountryInfo();
      this.initCustomersMasonry();
      this.initPluso();
      this.initZoom();
      _.delay(_.bind(function() {
        this.checkParams();
      }, this), 1000);
      this.initFacebook(document, 'script', 'facebook-jssdk');
      if (window.innerHeight < $document.outerHeight()) {
        this.onPageScroll();
      }

      if ($('#form_start').length > 0){
        new AmoValidate({
          form_selector: '#form_start',
          input_selector: '.form_start__type',
          submit_callback: 'form_start',
          checkbox_selector: '#free_assistance',
          checkbox_callback: 'assistance',
          check_email_existence: true
        });
      }

      if ($('#cloud_form').length) {
        new AmoValidate({
          form_selector: '#cloud_form',
          input_selector: '.form_start__type',
          submit_callback: 'cloud_form',
          check_email_existence: true
        });
      }

      if ($('#blog_sbcr_form').length > 0){
        new AmoValidate({
          form_selector: '#blog_sbcr_form',
          input_selector: '.blog_sbsr_input',
          submit_callback: 'form_blog_sbcr'
        });
      }
    },

    collapsing: function () {
      $document.on('click', '.colapse_toggler', function() {
        var $wrapper = $(this).closest('.colapse_block'),
            $content = $wrapper.find('.colapse_content'),
            $icon    = $wrapper.find('.icon-arrow-down');


        $icon.toggleClass('upward');
        $wrapper.toggleClass('expanded');
        $content.slideToggle();

        return false;
      });
    },

    checkParams: function () {
      if (this.getParameterByName('passwd')) {
        if ($body.hasClass('desktop')) {
          $('.page_header__auth_button').click();
          $('.js-form_auth__forgot').click();
        } else {
          $('#main-menu_button').click();
          $('#page_header__auth_button').click();
          $('.js-form_auth__forgot').click();
        }
      } else if (this.getParameterByName('try_auth')) {
        $('.page_header__auth_button').click();
      }
    },

    getParameterByName: function (name) {
      var url = window.location.href;
      name = name.replace(/[\[\]]/g, "\\$&");
      var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
        results = regex.exec(url);
      if (!results) return null;
      if (!results[2]) return '';
      return decodeURIComponent(results[2].replace(/\+/g, " "));
    },

    getCountryInfo: function() {
      var self = this;
      if (Cookies.get('amo_geo') === undefined) {
        $.ajax({
          dataType: "json",
          url: '/private/get_country.php?ip='+AMOCRM.user_ip,
          success: function(data) {
            Cookies.set('amo_geo', data, {expires: 90});
            self.workWithTimezone(data.time_zone);
            self.workWithCountry(data.country);
          }
        });
      } else {
        var amo_geo_info_json = Cookies.getJSON('amo_geo');
        this.workWithTimezone(amo_geo_info_json.time_zone);
        this.workWithCountry(amo_geo_info_json.country);
      }
    },

    workWithCountry: function (country) {
      if (country == '' || country == undefined || !country || country == null) {
        country = AMOCRM.geo.country;
      }
      $('.country_reg').val(country);
    },

    workWithTimezone: function (time_zone) {
      var timezone = time_zone;
      if (time_zone == '' || time_zone == undefined || !time_zone) {
        timezone = moment.tz.guess();
      }
      $('.timezone').val(timezone);
    },

    navigateToAnchor: function() {
      $('nav a[href*="#"]:not([href="#"])').click(function(event) {
        if (location.pathname.replace(/^\//,'') == this.pathname.replace(/^\//,'') && location.hostname == this.hostname) {
          var target = $(this.hash);
          target = target.length ? target : $('[name=' + this.hash.slice(1) +']');
          if (target.length) {
            event.preventDefault();
            if (history.pushState) {
              window.history.pushState(null, null, this.hash);
            } else {
              window.location.hash = this.hash;
            }
            $('nav a[href*="#"]').removeClass('active');
            $(this).addClass('active');
            $('html, body').animate({
              scrollTop: target.offset().top
            }, 1000, 'easeOutQuart');
            return;
          }
        }
      });
    },

    fileUploader: function() {
      var uploader = $('.js-file-upload');

      if (!uploader.length)
        return;

      uploader.each(function(i, el) {
        var uploader = $(this),
          input = uploader.find('.js-file-upload-input'),
          textfield = uploader.find('.js-file-upload-text'),
          clear = uploader.find('.js-file-upload-clear'),
          initialText = textfield.text();

        input.on('change', function(event) {
          var files = this.files,
            filenames = '';

          if (!files.length) {
            textfield.text(initialText);
            uploader.removeClass('is-not-empty');
            return;
          }

          for (var i = 0; i < files.length; i++) {
            filenames += files[i].name + ', ';
          }
          filenames = filenames.slice(0, -2);
          textfield.text(filenames);
          uploader.addClass('is-not-empty');
        });

        clear.on('click', function(event) {
          event.preventDefault();
          input.attr('value', '');
          textfield.text(initialText);
          uploader.removeClass('is-not-empty');
        });
      });
    },

    bindEvents: function () {
      var self = this;

      this.pricing_limitation_visible = false;

      var available;
      var percentage_of_page;
      var half_screen;
      var height;
      // var bottom_50_screen;

      $(window).scroll(function () {
        available = $(document).height();
        percentage_of_page = 0.30;
        half_screen = available * percentage_of_page;
        // bottom_50_screen = available * 0.6;
        height = $(window).scrollTop();
        if (height > half_screen /* && !(height > bottom_50_screen)*/) {
          $('.bottom-sub').show();
        } else {
          $('.bottom-sub').hide();
        }
      });

      $document.on('click', '.close_btn_blog', function (e) {
        $('.bottom-sub').slideUp();
        Cookies.set('amo_blog_sub', 1, {expires: 30});
        setTimeout(function () {
          $('.bottom-sub').remove();
        }, 1000);
      });

      $document.on("click",".share_btn", function (e) {
        e.preventDefault();
        var winHeight = 350,
          winWidth = 600,
          winTop = (screen.height / 2) - (winHeight / 2),
          winLeft = (screen.width / 2) - (winWidth / 2),
          link = $(this).data('share');
        window.open(link, 'sharer', 'top=' + winTop + ',left=' + winLeft + ',toolbar=0,status=0,width=' + winWidth + ',height=' + winHeight);
      });

      $document.on('click', '#pricing_limitation', function (e) {
        var $elem = $(e.currentTarget).find('.pricing_package_item__limitaion_desc');

        e.stopPropagation();

        // форсим reflow для анимации
        $elem.toggle();
        $elem[0].offsetWidth;
        $elem.toggleClass('pricing_package_item__limitaion_desc_visible');

        self.pricing_limitation_visible = !self.pricing_limitation_visible;
      }).on('click', 'body.buy', function(e) {
        if (self.pricing_limitation_visible) {
          $('#pricing_limitation')
            .find('.pricing_package_item__limitaion_desc')
            .toggle()
            .toggleClass('pricing_package_item__limitaion_desc_visible');

          self.pricing_limitation_visible = !self.pricing_limitation_visible;
        }
      }).on('click', '.pricing_package_item__limitaion_desc', function(e) {
        e.stopPropagation();
      }).on('click', '#change_password .get_password__button_submit', function(e) {
        var $inputs = $('#change_password .get_password__type'),
          $top_input = $inputs.eq(0),
          $input_error1 = $('#change_password .get_password__type_error').eq(0),
          $input_error2 = $('#change_password .get_password__type_error').eq(1),
          val1 = $top_input.val().trim(),
          val2 = $inputs.eq(1).val().trim(),
          has_errors = false;

        $input_error1.hide();
        $input_error2.hide();

        if (val1.length < 6) {
          $input_error1.html(AMOCRM.langs.SMALL_PASSWORD).show();
          has_errors = true;
        } else if (val1 !== val2) {
          $input_error2.html(AMOCRM.langs.WRONG_PASS_CONFIRMATION).show();
          has_errors = true;
        }

        if (has_errors) {
          e.preventDefault();
        }

        $inputs.toggleClass('form_auth__type_invalid', has_errors);
      }).on('input', '#change_password .get_password__type', function(e) {
        $('#change_password .get_password__type')
          .removeClass('form_auth__type_invalid');
        $('#change_password .get_password__type_error').hide();
      }).on('click', '.blog__btn_load_more', function (e) {
        self.getBlog(e);
      });


        //Подзагрузка блога по прокрутке
        if (window.location.pathname === '/blog/') {
            $window.on('scroll', _.bind(self.scrollGetBlog, self));
        }

      // Клики по тегам в блоге
      $('.blog__tag').click(function () {
        $('.blog__tag_active').removeClass('blog__tag_active');
        $(this).addClass('blog__tag_active');

        $('.blog__item').remove();
        self.getBlog.call($window);

      });

      //Шеры в blog detail

            $(document).scroll(function() {
              if($('.blog__social-likes')[0]) {
                if ($(window).scrollTop() > ($('.header-blog').height()+ 150) && $(window).scrollTop() < ($('.blog__social-likes').offset().top - 300)) {
                    $('.element__article__soc_buttons_blog').addClass('element__article__soc_buttons_blog_fixed');
                }
                else {
                    $('.element__article__soc_buttons_blog').removeClass('element__article__soc_buttons_blog_fixed');
                }
              }
            });

      // драг-скролл для страницы прайсинга
      $document.on('mousedown', '#compare__page', function (e) {
        e.preventDefault();

        self.drag.start = true;
        self.drag.x = e.pageX;
        self.drag.y = e.pageY;
        self.drag.top = $(this).scrollTop();
        self.drag.left = $(this).scrollLeft();

        $('#compare__page').addClass('start-drag');
      });

      $body.on('mousemove', function (e) {
        if (self.drag.start) {
          $('#compare__page').scrollTop(self.drag.top - e.pageY + self.drag.y);
          $('#compare__page').scrollLeft(self.drag.left - e.pageX + self.drag.x);
        }
      }).on('mouseup', function (e) {
        self.drag.start = false;

        $('#compare__page').removeClass('start-drag');
      });

      // стики меню
      $(document).on('touchmove scroll', _.debounce(function(e) {
        self.onPageScroll();
      }, 0));


      // переключение цен на странице прайсинга
      $('#pricing-compare, .page--pricing-archive')
        .on('click', '.currency_switcher__item', function(e) {
          var $this = $(e.currentTarget),
            $switcher = $this.closest('.currency_switcher'),
            currency = $this.data('currency'),
            parent = $switcher.data('parent'),
            item = $switcher.data('item');

          $switcher
            .children()
            .removeClass('active')
            .filter('.currency_switcher__item_' + currency)
            .addClass('active');

          $('.' + parent)
            .find('.' + item)
            .removeClass('active')
            .filter('.' + item + '_' + currency)
            .addClass('active');
        });
    },

    // стики меню показывается при скролле контента и скрывается,
    // если блок формы старт попадает в scroll view
    onPageScroll: function (e) {
      if ($body.hasClass('index-page')) {
        return;
      }

      var $top_menu = $('#top-menu'),
        $header = $('.content-block--trial .content-block__title_large'),
        header_offset = $header.length ? ($header.offset().top + $header.outerHeight()) : $document.outerHeight() + 1,
        scroll_pos = $window.scrollTop(),
        scroll_view = scroll_pos + window.innerHeight,
        header_h = $('.header__background').outerHeight();

      if (scroll_pos >= header_h && scroll_view < header_offset && scroll_pos > 100) {
        $top_menu.addClass('top-menu_blue');
      } else {
        $top_menu.removeClass('top-menu_blue');
      }
    },

    initBellowsConfig: function () {
      $('.bellows').bellows({
        singleItemOpen: false,
        duration: 200,
        easing: 'swing'
      });
    },

    initFancyBoxConfig: function () {
      $('.fancybox').fancybox({
        fitToView: true,
        width: '5560',
        height: '3150',
        maxWidth: '92%',
        maxHeight: '85%',
        autoSize: false,
        aspectRatio: true,
        closeClick: false,
        openEffect: 'none',
        closeEffect: 'none',
        padding: 0,
      });
    },

    initCustomersMasonry: function(){
      var $customers__comments = $('.customers__comments'),
        $grid = $customers__comments.masonry({
          itemSelector: '.customers__comment',
          gutter: '.gutter__sizer',
          percentPosition: true,
          transitionDuration: '0.1s'
        });

      $customers__comments.imagesLoaded(function(){
        $grid.masonry('layout');
      })
      $window.resize(function(){
        $grid.masonry('layout');
      })
    },



    scrollGetBlog:function(){
      if($(window).scrollTop() + $(window).height() >= $(document).height() - 200 && !this.get_blog_ajax) {
        this.getBlog.call(this);
      }
    },

    getBlog: function () {
      var self = this,
        button_html = '<div class="blog__btn_load_more" data-page="1"><span>БОЛЬШЕ ПОЛЕЗНЫХ МАТЕРИАЛОВ </span></div>',
        tag = $('.blog__tag_active').html(),
        first_load = $('.blog__item').length ? 0 : 1,
        $button,
        page_num;

      if (first_load) {
        if ($('.blog__btn_load_more').length === 0) {
          $('.page_section__inner').append(button_html);
        } else {
          $('.blog__btn_load_more').data('page', 1);
        }

      }

      $button = $('.blog__btn_load_more');
      page_num = $button.data('page');

      if ($button.length == 0 ){
        return
      }

      this.get_blog_ajax = true;

      $button.addClass('loading');
      if (tag === 'Все') {
        tag = '';
      }
      $.ajax({
        type: 'GET',
        url: '/private/get_blog.php?PAGEN_1=' + page_num + '&SECTION_ID=' + $('.page_section_blog').data('section') + '&TAG=' + tag + '&FIRST_LOAD=' + first_load
      }).done(function(data){
        var $content;

        data = $.parseJSON(data);
        $content = $(data.html);

        $('.blog__items').append($content);

        if (data.page == '0'){
          $button.remove();
        } else {
          $button.data('page', data.page);
        }
        self.get_blog_ajax = false;
        $button.removeClass('loading');

      }).fail(function(){
        self.get_blog_ajax = false;
        $button.removeClass('loading');
      });
    },



    initPluso: function(){
      if (window.pluso) if (typeof window.pluso.start == "function") return;
      if (window.ifpluso == undefined) {
        window.ifpluso = 1;
        var d = document, s = d.createElement('script'), g = 'getElementsByTagName';
        s.type = 'text/javascript';
        s.charset = 'UTF-8';
        s.async = true;
        s.src = ('https:' == window.location.protocol ? 'https' : 'http') + '://share.pluso.ru/pluso-like.js';
        var h = d[g]('body')[0];
        h.appendChild(s);
      }
    },

    initZoom:function() {
      var $jzoom = $('.jzoom');

      if($jzoom.length>0){
        $jzoom
          .wrap('<span style="display:inline-block"></span>')
          .css('display', 'block')
          .parent()
          .zoom({magnify: 0.75});
      }

      window.fotoramaDefaults = {
        width: '100%',
        nav: 'thumbs',
        arrows: true,
        click: true,
        swipe: false,
        thumbfit: 'cover',
        loop: true,
        thumbmargin: 10,
        thumbwidth: 128
      };
    },

    initFacebook: function(d, s, id){
      var language =  AMOCRM.geo.language === 'ru' ? 'ru_RU' : 'en_US',
        js, fjs = d.getElementsByTagName(s)[0];

      if (d.getElementById(id)) return;
      js = d.createElement(s); js.id = id;
      js.async = true;
      js.src = '//connect.facebook.net/'+language+'/all.js#xfbml=1';
      fjs.parentNode.insertBefore(js, fjs);
    }
  });

  return new App();
});

function ResizeMap() {
    var $widthForMaps = $('.contacts__list_title_capital').width();
    $('#map').attr("width", $widthForMaps);
}
function changeText() {
    var BodyWidth = $('body').width();
    if(BodyWidth <= 480) {
        $('.content-block__search-item_submit').val('найти');
        $('.content-block__search-item_input').attr('placeholder', 'Поиск');
    } else{
        $('.content-block__search-item_submit').val('найти проект');
        $('.content-block__search-item_input').attr('placeholder', 'Введите отрасль или название кейса/компанию');
    }
}
function resizeSlider_case() {
    var $heightSliderMin = $('.content-block__slider-content').height();
    var $PageCase = $('.content-block__slider-content').closest('.page--case');
    var $widthSliderMobile = $('.content-block__img_holder').width();
    if($PageCase) {
        if (($heightSliderMin > 984) && ($widthSliderMobile > 1120)) {
            $('.content-block__slider-wrapper').css({"min-height": $heightSliderMin + "px"});
        } else if (($heightSliderMin > 700) && ($widthSliderMobile == 1120)) {
            $('.content-block__slider-wrapper').css({"min-height": $heightSliderMin + "px"});
        } else if (($heightSliderMin < 984) && ($widthSliderMobile > 1120)) {
            $('.content-block__slider-wrapper').css({"min-height": "984px"});
        } else if (($heightSliderMin < 700) && ($widthSliderMobile == 1120)) {
            $('.content-block__slider-wrapper').css({"min-height": "700px"});
        }
    }
}

function ajaxCases() {
    $("#message_form__submit-button_case").on("click", function(event) {
        event.preventDefault();
        var sentText = document.getElementById('contacts__form_sent-text'),
            CurrentUrl = window.location.pathname,
            $data =  new FormData(),
            $file = $('.js-file-upload-input')[0].files,
            $nameSessiya = document.getElementById('sessid'),
            $nameCompany=document.getElementById('message_form__type_text'),
            $email=document.getElementById('message_form__type_email'),
            $phoneType=document.getElementById('message_form__type_phone');

         $.each($file, function(key, value) {
           $data.append(key, value);
        });
         $data.append('NAME_COMPANY',$nameCompany.value);
        $data.append('EMAIL_FROM',$email.value);
        $data.append('AUTHOR_PHONE_CASE',$phoneType.value);
        $data.append('apply','Y');
        $data.append('sessid',$nameSessiya.value);

        if(CurrentUrl.indexOf('cases') != -1) {
            CurrentUrl = '/cases/';
        } else {
            CurrentUrl = CurrentUrl;
        }

        $.ajax({
            url:  CurrentUrl + "ajax.php",
            type: "POST",
            data: $data,
            dataType: 'json',
            contentType: false,
            processData: false,
            success: function (data) {
                if(data.error) {
                    $('.message_form__sent-text ').addClass('message_form__sent-text_warning ');
                }else {
                    $('.message_form__sent-text ').removeClass('message_form__sent-text_warning ');
                }
                sentText.innerHTML = data.message;
            },
            error: function () {
                console.log("error");
            }
        });
    });
}

$(window).resize(function(){
    changeText();
    ResizeMap();
    resizeSlider_case();
});
$(document).ready(function() {
    $('.modalbox').fancybox({
        padding: 0
    });
    changeText();
    ajaxCases();
    ResizeMap();
    resizeSlider_case();
});

var AmoValidate = AMOCRM.AmoValidate || {};

document.addEventListener('DOMContentLoaded', function() {
  'use strict';

  var $html = $('html'),
      $body = $('body'),
      $document = $(document),
      $window = $(window),

      AmoPage;

  AMOCRM = AMOCRM || {};

  AmoPage = function () {
    this.modal_auth_open = false;
    this.user_select_open = false;
    this.main_menu_open = false;
    this.modal_open = false;

    this.$page_overlay = $('#page_overlay');
    this.$page_holder = $('.page_holder');
    this.$page_wrapper = $('.page_wrapper');
    this.$page_footer = $('.page_footer');
    this.$main_menu = $('#main-menu');
    this.$main_menu_button = $('#main-menu_button');

    this.$pricing_compare = $('#pricing-compare');

    this.$top_menu = $('#top-menu');
    this.$auth_form = $('#form_auth__holder');
    this.$reg_form = $('#new_try_now');
    this.$forgot_form = $('#send_password');
    this.$auth_form_block = this.$auth_form.find('.form_auth__auth_container');
    this.$auth_forgot_block = this.$auth_form.find('.form_auth__forgotpass_container');

    this.$header_buttons_block = $('#page_header__auth');
    this.$header_auth_button = this.$header_buttons_block.find('#page_header__auth_button');
    this.$header_trial_button = this.$header_buttons_block.find('.page_header__auth__trial');
    this.$header_logo = $('.page_header__logo');
    this.$header_nav = $('.page_header__navigation');

    this.$user_select = $('#user-select__header');
    this.$user_select_block = $('.user-select__block');

    this.$auth_email_input = $('.form_auth__type_email');
    this.$auth_pass_input = $('.form_auth__type_password');
    this.$forgot_pass_input = $('.form_auth__type_forgotpass-mail');



    this.init();
  };

  _.extend(AmoPage.prototype, {
    init: function() {
      $html.removeClass('no-js');
      this.setLayoutType();
      this.bindEvents();
      this.bindLayoutChange();

      if (this.$reg_form.hasClass('show-on-start')) {
        this.toggleRegForm();
      }

      if (this.$auth_form.hasClass('show-on-start')) {
        this.toggleAuthForm();
      }

      if (this.getQueryParam('logout') === 'yes') {
        history.pushState(null, "", location.href.split("?")[0]);
      }
    },

    // установим режим декстопа или мобилки
    setLayoutType: function () {
      enquire.register('screen and (max-width:800px)', {
        setup: _.bind(function () {
          $body.addClass('desktop');
          this.desktop = true;
        }, this),
        match: _.bind(function () {
          $body.addClass('mobile').removeClass('desktop');
          this.desktop = false;
        }, this),
        unmatch: _.bind(function () {
          $body.addClass('desktop').removeClass('mobile');
          this.desktop = true;
        }, this)
      });
    },

    // одинаковые собития на десктопе и мобилке имеют разную логику
    bindEvents: function() {
      var self = this;

      this.bindUserSelectEvents();
      this.bindPricingCompareEvents();
      this.bindAuthFormEvents();
      this.bindMainMenuEvents();
      this.bindRegFormEvents();

      $document.on('click', '.lang-switcher', function (e) {
        e.stopPropagation();
        $(this).toggleClass('lang-switcher_visible');
      }).on('keydown', function(e){
        if((self.modal_reg_open || self.modal_auth_open) && e.keyCode=='9' && !($('input:focus').length > 0 || $('.show_forgot_password_form:focus').length > 0 || $('.form_auth__forgotpass:focus').length > 0 || $('.form_auth__type_block_captcha:focus').length > 0)){
          e.preventDefault();
          $('input[type=email]').trigger('focus');
        }
      }).on('click', 'body.desktop, .main-menu__minor_item', function(e) {
        // клик по документу закрывает соотвествующие модалки
        if (self.user_select_open) {
          self.toggleUserSelect(e);
        } else if (self.main_menu_open) {
          self.toggleMainMenu();
        } else if (self.modal_auth_open) {
          self.toggleAuthForm(true);
        } else if (self.modal_reg_open) {
          self.toggleRegForm(true);
        } else if (self.modal_pricing_compare_open) {
          self.togglePricingCompare(true);
        }
      }).on('click', 'body.mobile', function(e) {
        // клик по документу закрывает соотвествующие модалки
        if (self.user_select_open) {
          self.toggleUserSelect(e);
          self.forceClose();
          self.onUserSelectScroll(true);
        } else if (self.modal_auth_open) {
          self.toggleAuthFormMobile();
          self.forceClose();
        } else if (self.modal_reg_open) {
          self.toggleRegForm(true);
        }
      });


      this.removeIOSRubberEffect(document.querySelector('.pricing-compare__holder'));
      this.removeIOSRubberEffect(document.querySelector('.new_try_now-wrapper'));
      this.removeIOSRubberEffect(document.querySelector('.top-menu'));
      this.removeIOSRubberEffect(document.querySelector('.user-select__modal_holder'));


      $document.on('touchmove', 'body.no-scroll', function(e) {
        // блокировка скролла боди через оверлей на iOS устройствах
        // если у контейнеров нет класса scrollable-wrapper или их
        // контект меньше высоты вьюпорта скролл превентится.
        // также отключает скролл body двумя пальцами
        // при скролле контента включается removeIOSRubberEffect
        if (self.modal_open && !$(e.target).closest('.iti-container').length) {
          var $wrapper = $(e.target).closest('.scrollable-wrapper');

          if (!$wrapper.length || ($wrapper.outerHeight() < window.innerHeight) || (e.originalEvent.touches.length > 1)) {
            e.preventDefault();
          }
        }
      })
       .on('touchmove', 'body.index-page:not(.no-scroll)', function(e) {
         if (!self.modal_open && ($body.outerHeight() < window.innerHeight) || ($body.width() < 1025 && ($body.outerHeight() <= window.innerHeight))) {
           e.preventDefault();
         }
       });
    },

    forceClose:function(){
      this.modal_open = false;
      this.main_menu_open = false;
      this.$main_menu_button.toggleClass('main-menu_button_active');
      this.$main_menu.removeClass('main-menu_visible');
      this.$header_buttons_block.removeClass('hidden');
      this.$top_menu.removeClass('top-menu_open');
      this.togglePageBlur();
    },

    // блюр страницы
    togglePageBlur: function() {
      this.scrollbar_w = this.getScrollbarWidth();
      this.page_scroll = this.checkScroll($html);
      this.$page_overlay.toggleClass('page_overlay_visible');
      this.$page_holder.toggleClass('blur-in');
      this.$page_footer.toggleClass('blur-in');
      this.$page_wrapper.toggleClass('blur-in');
      this.$header_nav.toggleClass('hidden');
      $body.toggleClass('no-scroll');
    },

    bindUserSelectEvents: function () {
      var self = this;

      $document.on('click', '.user-select__header', function(e) {
        // нужно, чтобы событие не проваливалось на документ
        // клик по документу закрывает модалки
        e.stopPropagation();
      }).on('click', '.js-user-select-current', function (e) {
        // показать список пользователей
        e.preventDefault();

        self.toggleUserSelect(e);
      }).on('touchend touchstart mousewheel', 'body.mobile .user-select__modal', _.debounce(function(e) {
        // скролл списка пользователей на мобильном устройстве
        // должен фейдить кнопку меню и лого, в момент,
        // когда их перекрывает
        if (AMOCRM.user_auth && self.user_select_open) {
          self.onUserSelectScroll();
        }
      }, 100));
    },

    // меню выбора пользователя
    // прячем кнопку, показываем фейк
    toggleUserSelect: function(e) {
      this.user_select_open = !this.user_select_open;
      this.modal_open = this.user_select_open;

      this.$user_select.toggleClass('user-select_hidden');
      $('.user-select__modal_holder').toggle();

      if (this.desktop) {
        this.togglePageBlur();
        this.$top_menu.toggleClass('blur-in');
      } else {
        this.$main_menu.toggleClass('main-menu_visible');
      }

      // позиционирование нужно для разных разрешений и для
      // разных положений меню
      if (this.user_select_open) {
        this.positionUserSelect();
      }
    },

    positionUserSelect: function () {
      var top = $('#user-select__header').offset().top - $(window).scrollTop();

      $('.user-select__modal_holder').scrollTop(0);
      $('.user-select__modal').css('paddingTop', top);
    },

    onUserSelectScroll: function (force_exit) {
      if (this.$user_select_block.offset().top < this.us_offset_base.top && !force_exit) {
        this.$main_menu_button.removeClass('fadeOut').addClass('fadeIn');
        this.$header_logo.removeClass('fadeOut').addClass('fadeIn');
      } else {
        this.$main_menu_button.removeClass('fadeIn').addClass('fadeOut');
        this.$header_logo.removeClass('fadeIn').addClass('fadeOut');
      }
    },

    modalToggleVisibility: function($elem) {
      /**
       * TODO
       * переписать для новой страницы
       */
      $elem.toggle();

      $('#form_start__type_email').toggle();
      this.$header_auth_button.toggleClass('invisible');
      this.$header_trial_button.toggleClass('invisible');

      this.$header_logo.toggleClass('blur-in');
      this.$main_menu_button.toggleClass('blur-in');
      this.togglePageBlur();
    },

    bindMainMenuEvents: function () {
      var self = this;

      $document.on('click', '.page_header__logo', function(e) {
        // нужно, чтобы событие не проваливалось на документ
        // клик по документу закрывает модалки
        e.stopPropagation();

        if ($body.hasClass('index-page')) {
          e.preventDefault();
        }
      }).on('click', '#main-menu', function(e) {
        // нужно, чтобы событие не проваливалось на документ
        // клик по документу закрывает модалки
        e.stopPropagation();
      }).on('click', 'body.desktop #main-menu_button', function (e) {
        // основное меню
        e.stopPropagation();

        self.toggleMainMenu();
      }).on('click', 'body.mobile #main-menu_button', function (e) {
        // основное меню
        e.stopPropagation();

        self.toggleMainMenuMobile();
      });
    },

    // основное меню
    toggleMainMenu: function() {
      var $this = this.$main_menu_button;

      this.modal_open = !$this.hasClass('main-menu_button_active');
      this.main_menu_open = this.modal_open;

      this.$main_menu_button.toggleClass('main-menu_button_active');
      this.$main_menu.toggleClass('main-menu_visible');
      this.$top_menu.toggleClass('top-menu_open', this.main_menu_open);
      if (this.desktop) {
        this.$header_buttons_block.toggleClass('hidden');
      }
      this.togglePageBlur();
    },

    // основное меню для мобилы
    toggleMainMenuMobile: function(keep_blur) {
      this.$main_menu_button.toggleClass('main-menu_button_active');
      this.$top_menu.toggleClass('top-menu_open');

      if (!keep_blur) {
        this.togglePageBlur();
      }

      if (!this.main_menu_open) {
        if (AMOCRM.user_auth) {
          this.us_offset_base = this.$user_select.offset();
        } else {
        }

        this.modal_open = true;
        this.main_menu_open = true;
        this.$main_menu.addClass('main-menu_visible');
      } else {
        if (this.user_select_open) {
          this.user_select_open = false;
          this.$user_select.toggleClass('user-select_hidden');
          $('.user-select__modal_holder').toggle();
          $body.removeClass('no-scroll');
        } else if (this.modal_auth_open) {
          this.toggleAuthFormMobile();
        }

        this.$main_menu.removeClass('main-menu_visible');
        this.$user_select.removeClass('user-select_hidden');
        $('.bellows__content-wrapper').hide()
          .closest('.bellows--is-open').removeClass('bellows--is-open');

        this.modal_open = false;
        this.main_menu_open = false;
      }
    },

    bindAuthFormEvents: function () {
      var self = this;

      this.auth_validate = new AmoValidate({
        form_selector: '#form_auth',
        input_selector: '.form_auth__type',
        submit_callback: 'form_auth'
      });

      this.auth_forgot = new AmoValidate({
        form_selector: '#send_password',
        input_selector: '.form_auth__type',
        submit_callback: 'form_forgot'
      });

      $document.on('click', 'body.desktop #page_header__auth_button', function(e) {
        e.stopPropagation();

        if (!self.modal_auth_open) {
          self.toggleAuthForm();
        }
      }).on('click', 'body.mobile #page_header__auth_button', function (e){
        e.stopPropagation();

        self.toggleAuthFormMobile();
      }).on('click', '#form_auth__holder', function(e) {
        // нужно, чтобы событие не проваливалось на документ
        // клик по документу закрывает модалки
        e.stopPropagation();
      }).on('click', 'body.mobile .form_auth__button_create-new', function (e) {
        // закрыть модалки, открыть форму регистрации
        e.stopPropagation();
        e.preventDefault();

        self.toggleAuthFormMobile();
        self.toggleMainMenuMobile();
        self.toggleRegForm();
      }).on('click', 'body.desktop .form_auth__button_create-new', function (e) {
        // закрыть модалки, открыть форму регистрации
        e.preventDefault();

        self.toggleAuthForm(true);
        self.toggleRegForm();
      }).on('click', 'body.desktop #developers-room-auth', function(e) {
        e.stopPropagation();

        if (!self.modal_auth_open) {
          self.toggleAuthForm();
        }
      }).on('click', 'body.mobile #developers-room-auth', function(e) {
        e.stopPropagation();

        self.toggleMainMenuMobile();
        self.toggleAuthFormMobile();
      });

      this.$auth_form.on('click', '.form_auth__button_authorize', function(e) {
        // переключение формы авторизации и восстановления пароля
        e.preventDefault();

        self.toggleAuthBlocksVisibility();
      }).on('click', '.js-form_auth__forgot', function(e) {
        // переключение формы авторизации и восстановления пароля
        e.preventDefault();

        self.toggleAuthBlocksVisibility(true);
      }).on('click', '.form_auth__button_close', function(e) {
        // закрыть меню авторизации
        e.stopPropagation();

        self.toggleAuthForm(true);
      });
    },

    // форма авторизации
    toggleAuthForm: function(force_exit, keep_blur) {
      this.$top_menu.toggleClass('blur-in top-menu_open');
      $('.user-select__modal_holder').toggle();

      if (!keep_blur) {
        this.togglePageBlur();
      }

      if (!this.modal_auth_open) {
        this.modal_auth_open = true;
        this.modal_open = true;
      } else if (force_exit) {

        this.modal_auth_open = false;
        this.modal_open = false;

        this.toggleAuthBlocksVisibility();
        this.$auth_form.find('.field').removeClass('invalid_field').removeClass('valid_field');
        this.$auth_form.find('input:not([name="captcha_sid"])').val('');
        this.$auth_form.find('.form_auth__notify').hide();
      }
      this.positionAuthForm();

      //фиксим скролл
      this.scrollFix($('.form_auth'));
    },

    // управляет анимацией отображением блока
    // авторизации или формой восст.пароля
    toggleAuthBlocksVisibility: function (show) {
      if (show) {
        this.$auth_form_block.addClass('hidden').css({opacity:0});
        this.$auth_forgot_block.removeClass('hidden').transition({opacity:1},300);
      } else {
        this.$auth_forgot_block.addClass('hidden').css({opacity:0});
        this.$auth_form_block.removeClass('hidden').transition({opacity:1},300);
      }
    },

    //Определим положение кнопки и отрисуем туда форму
    positionAuthForm: function () {
      $('.page_header__auth').removeClass('hidden');
      var button_pos_top = $('.page_header__auth_button').offset().top;
        this.$auth_form.css({top: button_pos_top - $(window.screen.height).scrollTop()});
      if (!this.desktop){
        $('.page_header__auth').addClass('hidden');
      }

    },

    // форма авторизации на мобиле
    toggleAuthFormMobile: function() {

      this.$main_menu.toggleClass('main-menu_visible');
      this.$user_select.toggleClass('user-select_visible');
      this.$header_buttons_block.toggleClass('hidden');

      $('.user-select__modal_holder').toggle();
      this.$user_select.toggleClass('user-select_visible');

      if (this.modal_auth_open) {

        this.$header_trial_button.removeClass('hidden');
        this.modal_auth_open = false;
        this.toggleAuthBlocksVisibility();

        this.$auth_form.find('.field').removeClass('invalid_field').removeClass('valid_field');
        this.$auth_form.find('input:not([name="captcha_sid"])').val('');
        this.$auth_form.find('.form_auth__notify').hide();
      } else {
        this.positionAuthForm();
        this.modal_auth_open = true;
      }

    },


    // события открывающие форму регистрации
    bindRegFormEvents: function () {
      var self = this;

      $document.on('click', '.page_header__auth__trial_link, .js-trial-link', function (e) {
        e.stopPropagation();
        e.preventDefault();

        if (self.modal_pricing_compare_open) {
          self.togglePricingCompare(true);
        } else if (self.modal_open) {
          self.toggleMainMenuMobile();
        }

        self.toggleRegForm();
      }).on('click', '.new_try_now__button_close', function(e) {
        e.preventDefault();
        self.toggleRegForm(true);
      }).on('click', 'body.desktop .new_try_now__inner, body.mobile .new_try_now', function (e) {
        // блокируем проваливание события на документ
        // клик по документу закрывает модалки
        e.stopPropagation();
      }).on('click', '.js-forgot_password_link', function(e) {
        // октрыть форму восст.пароля из формы реги
        self.showForgotPassFormFromReg();
      });
    },

    // модалка регистрации
    toggleRegForm: function(force_exit, keep_blur) {
      var $this = this.$header_auth_button;

      if (AMOCRM.user_auth) {
        return this.redirectToAccountList();
      }

      if (!this.modal_reg_open) {
        this.toggleRegFormVisibility(true);

        this.reg_view = new AmoValidate({
          form_selector: '#new_try_now__form',
          input_selector: '.new_try_now__reg.new_try_now__input',
          submit_callback: 'form_start',
          checkbox_selector: '#new_try_now__free_assist',
          checkbox_callback: 'assistance',
          check_email_existence: true,
          switch_mode: true,
          form_type: 'reg'
        });

        if (!$html.hasClass('touchevents')) {
          this.$reg_form.find('.try_now_mail').focus()
            .end().find('.selected-flag').prop('tabindex', '-1');
        }
      } else if (force_exit) {
        this.toggleRegFormVisibility(false, keep_blur);

        this.$header_buttons_block.removeClass('hidden');
        this.toggleAuthBlocksVisibility();

        this.reg_view.destroy(true);
        this.reg_view = null;
        this.$reg_form.find('.hide_when_email_exist').show();
        this.$reg_form.find('.show_when_email_exist').hide();
        this.$reg_form.find('.new_try_now__free_assist').prop('checked', true);
        // this.$reg_form.find('.field')
        //   .removeClass('invalid_field valid_field disabled_field')
        //   .find('input').val('');
      }
    },

    toggleRegFormVisibility: function (open, keep_blur) {
      this.modal_reg_open = open;
      this.modal_open = open;
      this.$reg_form.toggle();
      this.$top_menu.toggleClass('blur-in');

      if (!keep_blur) {
        this.togglePageBlur();
      }
    },

    // переход из формы реги в форму восст.пароля
    // без отключения блюра
    showForgotPassFormFromReg: function () {
      var email = this.$reg_form.find('#new_try_now_email_input').val();

      this.toggleRegForm(true, true);

      if (this.desktop) {
        this.toggleAuthForm(false, true);
      } else {
        this.toggleMainMenuMobile(true);
        this.toggleAuthFormMobile();
        this.toggleAuthBlocksVisibility(true);
      }

      this.$auth_form.find('.form_auth__type_forgotpass-mail')
        .val(email).trigger('input');
      this.toggleAuthBlocksVisibility(true);
    },

    // редирект на страницу аккаунта
    redirectToAccountList: function() {
      var reg_url = AMOCRM.geo.language === 'es' ? '/es/account/' : '/account/';

      window.location = window.location.origin + reg_url;
    },

    bindPricingCompareEvents: function () {
      $document.on('click', '.js-tarifes__pricing_compare', function (e) {
        // открыть модалку сравнения цен
        e.stopPropagation();
        e.preventDefault();

        self.togglePricingCompare();
      }).on('click', '.pricing-compare', function(e) {
        // события, проваливающиеся на документ, закрывают модалку
        e.stopPropagation();
      }).on('click', '#pricing-compare .form_auth__button_close', function(e) {
        // закрыть модалку сравнения цен
        e.stopPropagation();
        e.preventDefault();

        self.togglePricingCompare(true);
      });
    },

    togglePricingCompare: function (force_exit) {
      var self = this,
          prefix = AMOCRM.geo.language == 'es' ? '/es' : '';

      if (!this.modal_pricing_compare_open) {
        $.ajax({
          url: prefix + '/buy/compare/index.php?modal=true',
          method: 'GET',
        }).done(function (res) {

          self.modal_pricing_compare_open = true;
          self.modalToggleVisibility(self.$pricing_compare);
          self.$pricing_compare.find('.pricing-compare').html(res);
          $('.pricing-compare__holder').scrollTop(0);
          $body.addClass('no-scroll');
        });
      } else if (force_exit) {
        this.modal_pricing_compare_open = false;
        this.modalToggleVisibility(self.$pricing_compare);
        $body.removeClass('no-scroll');
      }
    },

    // переход из мобильной раскладки в десктоп и обратно
    bindLayoutChange: function() {
      enquire.register('screen and (max-width:800px)', {
        match: _.bind(function () {
          // прячем элементы аккордеона в верхнем меню
          $('.bellows__content-wrapper').hide();

          if (this.main_menu_open) {
            if (AMOCRM.user_auth) {
              // открыто меню, пользователь авторизован
              // this.$user_select.removeClass('blur-in');
              this.$header_buttons_block.removeClass('hidden');
            } else {
              // открыто основное меню, нет авторизации
              this.$header_buttons_block.removeClass('hidden');
            }
          } else if (this.modal_open && !this.modal_reg_open) {
            this.$main_menu_button.addClass('main-menu_button_active');
            this.main_menu_open = true;
            this.$top_menu.removeClass('blur-in');

            if (this.modal_auth_open) {
              // открыто меню авторизации
              this.$header_buttons_block.addClass('hidden');
              this.positionAuthForm();
            } else if (this.user_select_open) {
              // делаем фейд на лого и кнопки меню при скролле списка пользователей
              $('.user-select__modal_holder').scrollTop(0);
              this.$top_menu.addClass('top-menu_open');
              this.us_offset_base = this.$user_select.offset();
              this.positionUserSelect();
              this.onUserSelectScroll(true);
            }
          }
        }, this),

        unmatch: _.bind(function () {
          // прячем элементы аккордеона в верхнем меню
          $('.bellows__content-wrapper').hide();
          $('.main-menu__column-1 .bellows__content-wrapper').show();
          $('.main-menu__column-3 .top_menu_support .bellows__content-wrapper').show();

          if (this.main_menu_open && !this.modal_auth_open && !this.user_select_open) {
            if (AMOCRM.user_auth) {
              // открыто меню, пользователь авторизован
              // this.$user_select.addClass('blur-in');
              this.$header_buttons_block.addClass('hidden');
            } else {
              // открыто основное меню, нет авторизации
              this.$header_buttons_block.addClass('hidden');
            }
          } else if (this.modal_open && !this.modal_reg_open) {
            // this.$header_logo.addClass('blur-in');
            this.$main_menu_button.removeClass('main-menu_button_active');
            this.main_menu_open = false;
            $("#top-menu").addClass('blur-in');

            if (this.modal_auth_open) {
              // открыто меню авторизации
              this.positionAuthForm();
              this.$header_buttons_block.removeClass('hidden');
            } else if (this.user_select_open) {
              // убирает фейд с лого и кнопки меню при скролле списка пользователей
              this.positionUserSelect();
              this.onUserSelectScroll(true);
              this.$top_menu.removeClass('top-menu_open');
            }
          }
        }, this)
      });
    },


    // убирает оверскролл для iOS для модалок
    // http://blog.christoffer.me/six-things-i-learnt-about-ios-safaris-rubber-band-scrolling/
    removeIOSRubberEffect: function (element) {
      element.addEventListener('touchstart', function () {
        var top = element.scrollTop,
          totalScroll = element.scrollHeight,
          currentScroll = top + element.offsetHeight;

        if (top === 0) {
          element.scrollTop = 1;
        } else if (currentScroll === totalScroll) {
          element.scrollTop = top - 1;
        }
      });
    },

    getScrollbarWidth: function () {
      var parent, child, width;

      if (width === undefined) {
        parent = $('<div style="width:50px;height:50px;overflow:auto"><div/></div>').appendTo('body');
        child = parent.children();
        width = child.innerWidth() - child.height(99).innerWidth();
        parent.remove();
      }

      return width;
    },

    checkScroll: function(wrapper){
      if (window.innerHeight < wrapper.outerHeight()) {
        return true;
      }
      else {
        return false;
      }
    },


    scrollFix:function($wrapper){
      var after_scroll = this.checkScroll($wrapper);
      if (this.page_scroll && !after_scroll ){
        // console.log('надо сместить влево на величину скролла');
      } else if(!this.page_scroll && after_scroll){
        // console.log('надо сместить вправо на величину скрола');
      }
    },

    getQueryString: function () {
      var query_string = window.location.href.replace(/.*\?/, '').toString();

      if (query_string == window.location.href) {
        query_string = '';
      }

      return query_string ? query_string : '';
    },

    getQueryParam: function(name, query_string) {
      var regexS, regex, results;

      query_string = '?' + (query_string||decodeURIComponent(this.getQueryString()));
      name = name.replace(/[\[]/g, "\\\[").replace(/[\]]/g, "\\\]");

      regexS = "[\\?&]" + name + "=([^&#]*)";
      regex = new RegExp(regexS);
      results = regex.exec(query_string);

      if (results === null) {
        return false;
      } else {
        return decodeURIComponent(results[1].replace(/\+/g, " "));
      }
    },
  });

  return new AmoPage();
});


$(function () {
    var $name_input = $('.acc_name__input'),
        $address_input = $('.acc_adress__input'),
        $errors = $('.account_err'),
        $form = $('#new_acc_form'),
        $visitor_uid = $form.find('.visitor_uid'),
        flag_name_to_address = true;

    $('#submit_new_acc_form').click(function(e){
        e.preventDefault();

        $name_input.add($address_input).removeClass('has_error');

        if(address().length < 3 || name().length < 3 )
        {
            $errors.css('display', 'none');
            $('#ERROR_ACCOUNT_NAME_AND_SUBDOMAIN').css('display', 'inline-block');
            $name_input.add($address_input).addClass('has_error');
            return false;
        }
        $.ajax({
            url: "/account/check_subdomain.php",
            method: 'post',
            data: {subdomain: address()},
            success: function (response) {
                if(response == 'ok')
                {
                    $visitor_uid.val(AMOCRM.getVisitorUid());
                    $form.submit();
                }
                if(response == 'e')
                {
                    $errors.css('display', 'none');
                    $address_input.addClass('has_error');
                    $('#ERROR_SUBDOMAIN_EXISTS').css('display', 'inline-block');
                }
            },
        });
    });

    $address_input.focus(function(){
        flag_name_to_address = false;
        $name_input.add($address_input).removeClass('has_error');
        $errors.css('display', 'none');
    });
    $name_input.focus(function(){
        $name_input.add($address_input).removeClass('has_error');
        $errors.css('display', 'none');
    });

    $address_input.blur(function(){
        if(!address())
            flag_name_to_address = true;
    });
    $address_input.change(function(){
        $(this).val(trim_address(address()));
    });

    $name_input.keyup(function(){
        if(flag_name_to_address)
            name_to_address();
    });
    $name_input.keydown(function(){
        if(flag_name_to_address)
            name_to_address();
    });

    function address()
    {
        return $address_input.val();
    }
    function name()
    {
        return $name_input.val();
    }
    function name_to_address()
    {
        $address_input.val(trim_address(name()));
    }

    function trim_address(address)
    {
        address = address.toLowerCase();
        address = transliterate(address);
        address = address.replace(/ /g, '');
        address = address.replace(/-+/g, '');
        address = address.replace(/[^0-9-a-z]/g, '');
        address = address.replace(/^[^a-z0-9]+/g, '');
        address = address.replace(/[^a-z0-9]+$/g, '');
        address = address.substr(0, 50);
        return address;
    }

    function transliterate(word){
        var a = {"Ё":"YO","Й":"I","Ц":"TS","У":"U","К":"K","Е":"E","Н":"N","Г":"G","Ш":"SH","Щ":"SCH","З":"Z","Х":"H","Ъ":"'","ё":"yo","й":"i","ц":"ts","у":"u","к":"k","е":"e","н":"n","г":"g","ш":"sh","щ":"sch","з":"z","х":"h","ъ":"'","Ф":"F","Ы":"I","В":"V","А":"a","П":"P","Р":"R","О":"O","Л":"L","Д":"D","Ж":"ZH","Э":"E","ф":"f","ы":"i","в":"v","а":"a","п":"p","р":"r","о":"o","л":"l","д":"d","ж":"zh","э":"e","Я":"Ya","Ч":"CH","С":"S","М":"M","И":"I","Т":"T","Ь":"'","Б":"B","Ю":"YU","я":"ya","ч":"ch","с":"s","м":"m","и":"i","т":"t","ь":"'","б":"b","ю":"yu"};
        return word.split('').map(function (char) {
            return a[char] || char;
        }).join("");
    }

   function initNewAccForm() {
      var initer = document.getElementById('new_acc_form_init');
      if(initer === null) {
        return
      } else {
        var block = document.getElementById('new_acc_form');
        var error = document.querySelectorAll('.account_err');
        error.forEach(function(i){
          if(i.style.display == 'block') {
              initer.classList.add('active');
              block.classList.add('active');
          }
        })

        initer.addEventListener('click', function() {
            initer.classList.add('active');
            block.classList.add('active');
        })
      }
    } 
     
    initNewAccForm();
});

/**
 * Social Likes
 * http://sapegin.github.com/social-likes
 *
 * Sharing buttons for Russian and worldwide social networks.
 *
 * @requires jQuery
 * @author Artem Sapegin
 * @copyright 2014 Artem Sapegin (sapegin.me)
 * @license MIT
 */

/*global define:false, socialLikesButtons:false */

(function(factory) {  // Try to register as an anonymous AMD module
    if (typeof define === 'function' && define.amd) {
        define(['jquery'], factory);
    }
    else {
        factory(jQuery);
    }
}(function($, undefined) {

    'use strict';

    var prefix = 'social-likes';
    var classPrefix = prefix + '__';
    var openClass = prefix + '_opened';
    var protocol = location.protocol === 'https:' ? 'https:' : 'http:';
    var isHttps = protocol === 'https:';


    /**
     * Buttons
     */
    var services = {
        facebook: {
            counterUrl: 'https://graph.facebook.com/?id={url}',
            convertNumber: function(data) {
                return data.share.share_count;
            },
            popupUrl: 'https://www.facebook.com/sharer/sharer.php?u={url}',
            popupWidth: 600,
            popupHeight: 359
        },
        twitter: {
            counters: false,
            popupUrl: 'https://twitter.com/intent/tweet?url={url}&text={title}',
            popupWidth: 600,
            popupHeight: 250,
            click: function() {
                // Add colon to improve readability
                if (!/[\.\?:\-–—]\s*$/.test(this.options.title)) this.options.title += ':';
                return true;
            }
        },
        mailru: {
            counterUrl: protocol + '//connect.mail.ru/share_count?url_list={url}&callback=1&func=?',
            convertNumber: function(data) {
                for (var url in data) {
                    if (data.hasOwnProperty(url)) {
                        return data[url].shares;
                    }
                }
            },
            popupUrl: 'https://connect.mail.ru/share?share_url={url}&title={title}',
            popupWidth: 492,
            popupHeight: 500
        },
        vkontakte: {
            counterUrl: 'https://vk.com/share.php?act=count&url={url}&index={index}',
            counter: function(jsonUrl, deferred) {
                var options = services.vkontakte;
                if (!options._) {
                    options._ = [];
                    if (!window.VK) window.VK = {};
                    window.VK.Share = {
                        count: function(idx, number) {
                            options._[idx].resolve(number);
                        }
                    };
                }

                var index = options._.length;
                options._.push(deferred);
                $.getScript(makeUrl(jsonUrl, {index: index}))
                    .fail(deferred.reject);
            },
            popupUrl: 'https://vk.com/share.php?url={url}&title={title}',
            popupWidth: 655,
            popupHeight: 450
        },
        odnoklassniki: {
            counterUrl: protocol + '//connect.ok.ru/dk?st.cmd=extLike&ref={url}&uid={index}',
            counter: function(jsonUrl, deferred) {
                var options = services.odnoklassniki;
                if (!options._) {
                    options._ = [];
                    if (!window.ODKL) window.ODKL = {};
                    window.ODKL.updateCount = function(idx, number) {
                        options._[idx].resolve(number);
                    };
                }

                var index = options._.length;
                options._.push(deferred);
                $.getScript(makeUrl(jsonUrl, {index: index}))
                    .fail(deferred.reject);
            },
            popupUrl: 'https://connect.ok.ru/dk?st.cmd=WidgetSharePreview&service=odnoklassniki&st.shareUrl={url}',
            popupWidth: 580,
            popupHeight: 336
        },
        plusone: {
            counterUrl: protocol + '//share.yandex.ru/gpp.xml?url={url}&callback=?',
            convertNumber: function(number) {
                return parseInt(number.replace(/\D/g, ''), 10);
            },
            popupUrl: 'https://plus.google.com/share?url={url}',
            popupWidth: 500,
            popupHeight: 550
        },
        pinterest: {
            counterUrl: protocol + '//api.pinterest.com/v1/urls/count.json?url={url}&callback=?',
            convertNumber: function(data) {
                return data.count;
            },
            popupUrl: 'https://pinterest.com/pin/create/button/?url={url}&description={title}',
            popupWidth: 740,
            popupHeight: 550
        }
    };


    /**
     * Counters manager
     */
    var counters = {
        promises: {},
        fetch: function(service, url, extraOptions) {
            if (!counters.promises[service]) counters.promises[service] = {};
            var servicePromises = counters.promises[service];

            if (!extraOptions.forceUpdate && servicePromises[url]) {
                return servicePromises[url];
            }
            else {
                var options = $.extend({}, services[service], extraOptions);
                var deferred = $.Deferred();
                var jsonUrl = options.counterUrl && makeUrl(options.counterUrl, {url: url});

                if (jsonUrl && $.isFunction(options.counter)) {
                    options.counter(jsonUrl, deferred);
                }
                else if (options.counterUrl) {
                    $.getJSON(jsonUrl)
                        .done(function(data) {
                            try {
                                var number = data;
                                if ($.isFunction(options.convertNumber)) {
                                    number = options.convertNumber(data);
                                }
                                deferred.resolve(number);
                            }
                            catch (e) {
                                deferred.reject();
                            }
                        })
                        .fail(deferred.reject);
                }
                else {
                    deferred.reject();
                }

                servicePromises[url] = deferred.promise();
                return servicePromises[url];
            }
        }
    };


    /**
     * jQuery plugin
     */
    $.fn.socialLikes = function(options) {
        return this.each(function() {
            var elem = $(this);
            var instance = elem.data(prefix);
            if (instance) {
                if ($.isPlainObject(options)) {
                    instance.update(options);
                }
            }
            else {
                instance = new SocialLikes(elem, $.extend({}, $.fn.socialLikes.defaults, options, dataToOptions(elem)));
                elem.data(prefix, instance);
            }
        });
    };

    $.fn.socialLikes.defaults = {
        url: window.location.href.replace(window.location.hash, ''),
        title: document.title,
        counters: true,
        zeroes: false,
        wait: 500,  // Show buttons only after counters are ready or after this amount of time
        timeout: 10000,  // Show counters after this amount of time even if they aren’t ready
        popupCheckInterval: 500,
        singleTitle: 'Share'
    };

    function SocialLikes(container, options) {
        this.container = container;
        this.options = options;
        this.init();
    }

    SocialLikes.prototype = {
        init: function() {
            // Add class in case of manual initialization
            this.container.addClass(prefix);

            this.single = this.container.hasClass(prefix + '_single');

            this.initUserButtons();

            this.countersLeft = 0;
            this.number = 0;
            this.container.on('counter.' + prefix, $.proxy(this.updateCounter, this));

            var buttons = this.container.children();

            this.makeSingleButton();

            this.buttons = [];
            buttons.each($.proxy(function(idx, elem) {
                var button = new Button($(elem), this.options);
                this.buttons.push(button);
                if (button.options.counterUrl) this.countersLeft++;
            }, this));

            if (this.options.counters) {
                this.timer = setTimeout($.proxy(this.appear, this), this.options.wait);
                this.timeout = setTimeout($.proxy(this.ready, this, true), this.options.timeout);
            }
            else {
                this.appear();
            }
        },
        initUserButtons: function() {
            if (!this.userButtonInited && window.socialLikesButtons) {
                $.extend(true, services, socialLikesButtons);
            }
            this.userButtonInited = true;
        },
        makeSingleButton: function() {
            if (!this.single) return;

            var container = this.container;
            container.addClass(prefix + '_vertical');
            container.wrap($('<div>', {'class': prefix + '_single-w'}));
            container.wrapInner($('<div>', {'class': prefix + '__single-container'}));
            var wrapper = container.parent();

            // Widget
            var widget = $('<div>', {
                'class': getElementClassNames('widget', 'single')
            });
            var button = $(template(
                '<div class="{buttonCls}">' +
                '<span class="{iconCls}"></span>' +
                '{title}' +
                '</div>',
                {
                    buttonCls: getElementClassNames('button', 'single'),
                    iconCls: getElementClassNames('icon', 'single'),
                    title: this.options.singleTitle
                }
            ));
            widget.append(button);
            wrapper.append(widget);

            widget.on('click', function() {
                var activeClass = prefix + '__widget_active';
                widget.toggleClass(activeClass);
                if (widget.hasClass(activeClass)) {
                    container.css({left: -(container.width()-widget.width())/2,  top: -container.height()});
                    showInViewport(container);
                    closeOnClick(container, function() {
                        widget.removeClass(activeClass);
                    });
                }
                else {
                    container.removeClass(openClass);
                }
                return false;
            });

            this.widget = widget;
        },
        update: function(options) {
            if (!options.forceUpdate && options.url === this.options.url) return;

            // Reset counters
            this.number = 0;
            this.countersLeft = this.buttons.length;
            if (this.widget) this.widget.find('.' + prefix + '__counter').remove();

            // Update options
            $.extend(this.options, options);
            for (var buttonIdx = 0; buttonIdx < this.buttons.length; buttonIdx++) {
                this.buttons[buttonIdx].update(options);
            }
        },
        updateCounter: function(e, service, number) {
            number = number || 0;

            if (number || this.options.zeroes) {
                this.number += number;
                if (this.single) {
                    this.getCounterElem().text(this.number);
                }
            }

            this.countersLeft--;

            if (this.countersLeft === 0) {
                this.appear();
                this.ready();
            }
        },
        appear: function() {
            this.container.addClass(prefix + '_visible');
        },
        ready: function(silent) {
            if (this.timeout) {
                clearTimeout(this.timeout);
            }
            this.container.addClass(prefix + '_ready');
            if (!silent) {
                this.container.trigger('ready.' + prefix, this.number);
            }
        },
        getCounterElem: function() {
            var counterElem = this.widget.find('.' + classPrefix + 'counter_single');
            if (!counterElem.length) {
                counterElem = $('<span>', {
                    'class': getElementClassNames('counter', 'single')
                });
                this.widget.append(counterElem);
            }
            return counterElem;
        }
    };


    function Button(widget, options) {
        this.widget = widget;
        this.options = $.extend({}, options);
        this.detectService();
        if (this.service) {
            this.init();
        }
    }

    Button.prototype = {
        init: function() {
            this.detectParams();
            this.initHtml();
            setTimeout($.proxy(this.initCounter, this), 0);
        },

        update: function(options) {
            $.extend(this.options, {forceUpdate: false}, options);
            this.widget.find('.' + prefix + '__counter').remove();  // Remove old counter
            this.initCounter();
        },

        detectService: function() {
            var service = this.widget.data('service');
            if (!service) {
                // class="facebook"
                var node = this.widget[0];
                var classes = node.classList || node.className.split(' ');
                for (var classIdx = 0; classIdx < classes.length; classIdx++) {
                    var cls = classes[classIdx];
                    if (services[cls]) {
                        service = cls;
                        break;
                    }
                }
                if (!service) return;
            }
            this.service = service;
            $.extend(this.options, services[service]);
        },

        detectParams: function() {
            var data = this.widget.data();

            // Custom page counter URL or number
            if (data.counter) {
                var number = parseInt(data.counter, 10);
                if (isNaN(number)) {
                    this.options.counterUrl = data.counter;
                }
                else {
                    this.options.counterNumber = number;
                }
            }

            // Custom page title
            if (data.title) {
                this.options.title = data.title;
            }

            // Custom page URL
            if (data.url) {
                this.options.url = data.url;
            }
        },

        initHtml: function() {
            var options = this.options;
            var widget = this.widget;

            // Old initialization HTML
            var a = widget.find('a');
            if (a.length) {
                this.cloneDataAttrs(a, widget);
            }

            // Button
            var button = $('<span>', {
                'class': this.getElementClassNames('button'),
                'html': widget.html()
            });
            if (options.clickUrl) {
                var url = makeUrl(options.clickUrl, {
                    url: options.url,
                    title: options.title
                });
                var link = $('<a>', {
                    href: url
                });
                this.cloneDataAttrs(widget, link);
                widget.replaceWith(link);
                this.widget = widget = link;
            }
            else {
                widget.on('click', $.proxy(this.click, this));
            }

            widget.removeClass(this.service);
            widget.addClass(this.getElementClassNames('widget'));

            // Icon
            button.prepend($('<span>', {'class': this.getElementClassNames('icon')}));

            widget.empty().append(button);
            this.button = button;
        },

        initCounter: function() {
            if (this.options.counters) {
                if (this.options.counterNumber) {
                    this.updateCounter(this.options.counterNumber);
                }
                else {
                    var extraOptions = {
                        counterUrl: this.options.counterUrl,
                        forceUpdate: this.options.forceUpdate
                    };
                    counters.fetch(this.service, this.options.url, extraOptions)
                        .always($.proxy(this.updateCounter, this));
                }
            }
        },

        cloneDataAttrs: function(source, destination) {
            var data = source.data();
            for (var key in data) {
                if (data.hasOwnProperty(key)) {
                    destination.data(key, data[key]);
                }
            }
        },

        getElementClassNames: function(elem) {
            return getElementClassNames(elem, this.service);
        },

        updateCounter: function(number) {
            number = parseInt(number, 10) || 0;

            var params = {
                'class': this.getElementClassNames('counter'),
                'text': number
            };
            if (!number && !this.options.zeroes) {
                params['class'] += ' ' + prefix + '__counter_empty';
                params.text = '';
            }
            var counterElem = $('<span>', params);
            this.widget.append(counterElem);

            this.widget.trigger('counter.' + prefix, [this.service, number]);
        },

        click: function(e) {
            var options = this.options;
            var process = true;
            if ($.isFunction(options.click)) {
                process = options.click.call(this, e);
            }
            if (process) {
                var url = makeUrl(options.popupUrl, {
                    url: options.url,
                    title: options.title
                });
                url = this.addAdditionalParamsToUrl(url);
                this.openPopup(url, {
                    width: options.popupWidth,
                    height: options.popupHeight
                });
            }
            return false;
        },

        addAdditionalParamsToUrl: function(url) {
            var params = $.param($.extend(this.widget.data(), this.options.data));
            if ($.isEmptyObject(params)) return url;
            var glue = url.indexOf('?') === -1 ? '?' : '&';
            return url + glue + params;
        },

        openPopup: function(url, params) {
            var left = Math.round(screen.width/2 - params.width/2);
            var top = 0;
            if (screen.height > params.height) {
                top = Math.round(screen.height/3 - params.height/2);
            }

            var win = window.open(url, 'sl_' + this.service, 'left=' + left + ',top=' + top + ',' +
                'width=' + params.width + ',height=' + params.height + ',personalbar=0,toolbar=0,scrollbars=1,resizable=1');
            if (win) {
                win.focus();
                this.widget.trigger('popup_opened.' + prefix, [this.service, win]);
                var timer = setInterval($.proxy(function() {
                    if (!win.closed) return;
                    clearInterval(timer);
                    this.widget.trigger('popup_closed.' + prefix, this.service);
                }, this), this.options.popupCheckInterval);
            }
            else {
                location.href = url;
            }
        }
    };


    /**
     * Helpers
     */

    // Camelize data-attributes
    function dataToOptions(elem) {
        function upper(m, l) {
            return l.toUpper();
        }
        var options = {};
        var data = elem.data();
        for (var key in data) {
            var value = data[key];
            if (value === 'yes') value = true;
            else if (value === 'no') value = false;
            options[key.replace(/-(\w)/g, upper)] = value;
        }
        return options;
    }

    function makeUrl(url, context) {
        return template(url, context, encodeURIComponent);
    }

    function template(tmpl, context, filter) {
        return tmpl.replace(/\{([^\}]+)\}/g, function(m, key) {
            // If key doesn't exists in the context we should keep template tag as is
            return key in context ? (filter ? filter(context[key]) : context[key]) : m;
        });
    }

    function getElementClassNames(elem, mod) {
        var cls = classPrefix + elem;
        return cls + ' ' + cls + '_' + mod;
    }

    function closeOnClick(elem, callback) {
        function handler(e) {
            if ((e.type === 'keydown' && e.which !== 27) || $(e.target).closest(elem).length) return;
            elem.removeClass(openClass);
            doc.off(events, handler);
            if ($.isFunction(callback)) callback();
        }
        var doc = $(document);
        var events = 'click touchstart keydown';
        doc.on(events, handler);
    }

    function showInViewport(elem) {
        var offset = 10;
        if (document.documentElement.getBoundingClientRect) {
            var left = parseInt(elem.css('left'), 10);
            var top = parseInt(elem.css('top'), 10);

            var rect = elem[0].getBoundingClientRect();
            if (rect.left < offset)
                elem.css('left', offset - rect.left + left);
            else if (rect.right > window.innerWidth - offset)
                elem.css('left', window.innerWidth - rect.right - offset + left);

            if (rect.top < offset)
                elem.css('top', offset - rect.top + top);
            else if (rect.bottom > window.innerHeight - offset)
                elem.css('top', window.innerHeight - rect.bottom - offset + top);
        }
        elem.addClass(openClass);
    }


    /**
     * Auto initialization
     */
    $(function() {
        $('.' + prefix).socialLikes();
    });

}));
$(function () {
    var  is_mobile = window.matchMedia('(max-width: 1023px)'),
        mobile_open_tag = $('.blog__tag_open'),
        tags_container = $('.blog__tags'),
        invisible_tags = $('.blog__tag').slice(5),
        blog_tags = $('.blog__tags');

    if (is_mobile.matches) {
        invisible_tags.addClass('hidden');
        mobile_open_tag.removeClass('hidden');
    }

    $(window).resize(function () {
        if (is_mobile.matches) {
            invisible_tags.addClass('hidden');
            mobile_open_tag.css('display', 'inline-block').removeClass('hidden');
        } else {
            invisible_tags.removeClass('hidden');
            mobile_open_tag.addClass('hidden');
        }
    });

    mobile_open_tag.on('click', function () {
        $(this).hide();
        tags_container.find('.hidden').removeClass('hidden');
    });

    blog_tags.ready(function () {
        var regex = /tag=([^&]+)/g;

        if (window.location.search.indexOf('tag=') !== -1) {
           var name = decodeURI(regex.exec(window.location.search)[1]);
            $(".blog__tag[data-tag-id=" + name + "]").click();
        }

    });
});
